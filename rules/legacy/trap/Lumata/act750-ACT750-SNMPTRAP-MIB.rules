    $OS_EventId = "SNMPTRAP-act750-ACT750-SNMPTRAP-MIB-segmentation";

    $Event->{AlarmGroup} = "segmentation";
    $Event->{SubAlarmGroup} = $trapSeverity . $sourceComponent . $componentInfo2;
    $Event->{Summary} = "Segmentation: " . $componentInfo2;

    $DEFAULT_Severity = 3;
    $DEFAULT_Type = 2;
    $DEFAULT_ExpireTime = 0;

    $Event->{AlarmKey} = $Event->{Node} . " " . $Event->{SubAlarmGroup} . " " . $Event->{AlarmGroup} . " " . $DEFAULT_Type . " " . $Event->{Agent} . " " . $Event->{SubMethod} . " " . $specific;

} elsif ( $specific == "99" ) {    ### - test
    #######################
    #
    # Trap related to test. Can be used for test purpose only.
    #
    # $value[1] = trapSeverity
    #
    #      mandatory
    #
    # $value[2] = sourceComponent
    #
    #
    # $value[3] = componentInfo
    #
    #
    #######################

    $trapSeverity = lookup( $value[1], 'act750-ACT750-SNMPTRAP-MIB-trapSeverity' );
    $sourceComponent = $value[2];
    $componentInfo = $value[3];
    $componentInfo2 = extract( $componentInfo, '(.*),.*' );
    $OS_EventId = "SNMPTRAP-act750-ACT750-SNMPTRAP-MIB-test";

    $Event->{AlarmGroup} = "test";
    $Event->{SubAlarmGroup} = $trapSeverity . $sourceComponent . $componentInfo2;
    $Event->{Summary} = "Test: " . $componentInfo2;

    $DEFAULT_Severity = 3;
    $DEFAULT_Type = 2;
    $DEFAULT_ExpireTime = 0;

    $Event->{AlarmKey} = $Event->{Node} . " " . $Event->{SubAlarmGroup} . " " . $Event->{AlarmGroup} . " " . $DEFAULT_Type . " " . $Event->{Agent} . " " . $Event->{SubMethod} . " " . $specific;

} else {
    $Event->{Summary} = "Unknown Specific Trap Number (" . $specific . ") Received for Enterprise " . $enterprise;
    $Event->{Severity} = 1;
    $Event->{AlarmKey} = $Event->{Node} . " " . $Event->{Agent} . " " . $Event->{SubMethod} . " " . $enterprise . " " . $generic . " " . $specific;
}

if (0) {   # Dummy
} elsif ( $trapSeverity eq "CLEAR" ) {
    $Event->{Severity} = 0;
    $Event->{AlarmType} = 1;
} elsif ( $trapSeverity eq "INDETERMINATE" ) {
    $Event->{Severity} = 0;
    $Event->{AlarmType} = 2;
} elsif ( $trapSeverity eq "INFORMATIVE" ) {
    $Event->{Severity} = 2;
    $Event->{AlarmType} = 2;
} elsif ( $trapSeverity eq "WARNING" ) {
    $Event->{Severity} = 3;
    $Event->{AlarmType} = 2;
} elsif ( $trapSeverity eq "SERIOUS" ) {
    $Event->{Severity} = 4;
    $Event->{AlarmType} = 2;
} elsif ( $trapSeverity eq "CRITICAL" ) {
    $Event->{Severity} = 5;
    $Event->{AlarmType} = 2;
} else {
