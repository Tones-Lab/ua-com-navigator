PORT=3001
NODE_ENV=development
FRONTEND_URL=http://localhost:5173
LOG_LEVEL=info

# HTTPS (optional)
SSL_KEY_PATH=/opt/assure1/etc/ssl/Web.key
SSL_CERT_PATH=/opt/assure1/etc/ssl/Web.crt
COOKIE_SECURE=false

# Auth method feature flags
UA_AUTH_BASIC_ENABLED=true
UA_AUTH_CERT_ENABLED=true

# TLS options (for UA API integration/testing)
UA_TLS_INSECURE=true
UA_TLS_CERT_PATH=
UA_TLS_KEY_PATH=
UA_TLS_CA_PATH=

# UA DB query options
UA_DB_QUERY_ENDPOINT=/database/queryTools/executeQuery
UA_DB_QUERY_NAME=Event
UA_DB_QUERY_ID=
UA_DB_QUERY_SHARD=
UA_DB_QUERY_LIMIT=100

# UA Server Configuration (will be loaded from database/config in production)
# UA_DEFAULT_HOST=ua.example.com
# UA_DEFAULT_PORT=8080

# COMs indexing
COMS_ROOT=/root/navigator/coms
COMS_PATH_PREFIX=id-core/default/processing/event/fcom/_objects
SEARCH_MAX_CONTENT_BYTES=5242880

# Events schema cache
EVENTS_SCHEMA_TTL_MS=900000
EVENTS_SCHEMA_PATH=

# Cache freshness (stale-while-revalidate)
CACHE_TTL_MS=3600000
SEARCH_CACHE_TTL_MS=3600000
OVERVIEW_CACHE_TTL_MS=3600000
FOLDER_OVERVIEW_TTL_MS=3600000

# Overview cache
OVERVIEW_PAGE_LIMIT=500
OVERVIEW_REFRESH_INTERVAL_MS=600000

# Search cache
SEARCH_REFRESH_INTERVAL_MS=600000

# Retry/backoff for UA list/read
SEARCH_LIST_RETRIES=3
SEARCH_LIST_RETRY_DELAY_MS=500
SEARCH_READ_RETRIES=3
SEARCH_READ_RETRY_DELAY_MS=500
OVERVIEW_LIST_RETRIES=3
OVERVIEW_LIST_RETRY_DELAY_MS=500
OVERVIEW_READ_RETRIES=3
OVERVIEW_READ_RETRY_DELAY_MS=500
FOLDER_LIST_RETRIES=3
FOLDER_LIST_RETRY_DELAY_MS=500
FOLDER_READ_RETRIES=3
FOLDER_READ_RETRY_DELAY_MS=500

# Cache warmup (startup)
CACHE_REFRESH_INTERVAL_MS=600000
UA_BOOTSTRAP_SERVER_ID=
UA_BOOTSTRAP_AUTH_METHOD=basic
UA_BOOTSTRAP_USERNAME=
UA_BOOTSTRAP_PASSWORD=
UA_BOOTSTRAP_CERT_PATH=
UA_BOOTSTRAP_KEY_PATH=
UA_BOOTSTRAP_CA_CERT_PATH=

# MIB tooling
A1BASEDIR=/opt/assure1
UA_MIB_DIR=
UA_MIB2FCOM_BIN=
UA_SNMP_TRAP_CMD=snmptrap
MIBS=
