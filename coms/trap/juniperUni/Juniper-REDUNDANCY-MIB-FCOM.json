		{
			"@objectName" : "Juniper-REDUNDANCY-MIB::juniRedundancyColdSwitchoverNotification",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated in a dual SRP system when control",
				"transfers from one to SRP to the other in file system synchronization",
				"mode or when high availability is not operational. This notification",
				"is sent by the newly active SRP immediately following the switchover",
				"in which configuration is preserved but volatile state is lost. "
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "juniRedundancyColdSwitchoverNotification",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "ActiveSlot:$v1",
				"Summary" : "Redundancy cold switchover"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD Juniper-REDUNDANCY-MIB::juniRedundancyColdSwitchoverNotification Juniper-REDUNDANCY-MIB::juniRedundancyActiveSlot i 0 Juniper-REDUNDANCY-MIB::juniRedundancyLastResetReason i 1",
			"trap" : {
				"name" : "Juniper-REDUNDANCY-MIB::juniRedundancyColdSwitchoverNotification",
				"oid" : "1.3.6.1.4.1.4874.2.2.74.0.1",
				"variables" : [
					{
						"description" : [
							"A unique identifier for active SRP slot."
						],
						"name" : "Juniper-REDUNDANCY-MIB::juniRedundancyActiveSlot",
						"oid" : "1.3.6.1.4.1.4874.2.2.74.1.1.1",
						"valueType" : "INTEGER32"
					},
					{
						"description" : [
							"The reason for the last SRP reset."
						],
						"enums" : {
							"1" : "none",
							"2" : "notKnown",
							"3" : "userInitiated"
						},
						"name" : "Juniper-REDUNDANCY-MIB::juniRedundancyLastResetReason",
						"oid" : "1.3.6.1.4.1.4874.2.2.74.1.1.5",
						"valueType" : "INTEGER"
					}
				]
			}
		},
