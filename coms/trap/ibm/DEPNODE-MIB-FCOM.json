		{
			"@objectName" : "DEPNODE-MIB::switchInfoNeeded",
			"certification" : "STANDARD",
			"description" : [
				"A switchInfoNeeded trap signifies that the sending",
				"protocol entity needs SP switch configuration",
				"information for a dependent secondary node within",
				"the sender's configuration."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "switchInfoNeeded",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "SP switch configuration needed"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD DEPNODE-MIB::switchInfoNeeded DEPNODE-MIB::ibmSPDepNodeName s EXAMPLE",
			"trap" : {
				"name" : "DEPNODE-MIB::switchInfoNeeded",
				"oid" : "1.3.6.1.4.1.2.6.117.4.0.1",
				"variables" : [
					{
						"description" : [
							"Identifier assigned to this node in its",
							"system's administrative environment."
						],
						"name" : "DEPNODE-MIB::ibmSPDepNodeName",
						"oid" : "1.3.6.1.4.1.2.6.117.4.1.1.1",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
		{
			"@objectName" : "DEPNODE-MIB::switchConfigState",
			"certification" : "STANDARD",
			"description" : [
				"A switchConfigState trap indicates the final",
				"configuration state of a dependent secondary node's",
				"SP switch interface."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1==1 || $v1==2 || $v1==3 || $v1==4 || $v1==5) ? 2 : ($v1==6 ) ? 1 : 3"
				},
				"EventType" : "switchConfigState",
				"ExpireTime" : {
					"eval" : "($v1==1 || $v1==2 || $v1==3 || $v1==4 || $v1==5) ? 86400 : ($v1==6 ) ? 3600 : 3600"
				},
				"Severity" : {
					"eval" : "($v1==1 || $v1==2 || $v1==3 || $v1==4 || $v1==5) ? 4 : ($v1==6 ) ? 0 : 2"
				},
				"SubNode" : "Device",
				"Summary" : "SP switch interface state is $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD DEPNODE-MIB::switchConfigState DEPNODE-MIB::ibmSPDepConfigState i 1",
			"trap" : {
				"name" : "DEPNODE-MIB::switchConfigState",
				"oid" : "1.3.6.1.4.1.2.6.117.4.0.2",
				"variables" : [
					{
						"description" : [
							"This value indicates the final configuration state",
							"of the corresponding SP switch interface."
						],
						"enums" : {
							"1" : "notConfigured",
							"2" : "firmwareLoadFailed",
							"3" : "driverLoadFailed",
							"4" : "diagnosticFailed",
							"5" : "microcodeLoadFailed",
							"6" : "fullyConfigured"
						},
						"name" : "DEPNODE-MIB::ibmSPDepConfigState",
						"oid" : "1.3.6.1.4.1.2.6.117.4.1.1.10",
						"valueType" : "INTEGER"
					}
				]
			}
		},
