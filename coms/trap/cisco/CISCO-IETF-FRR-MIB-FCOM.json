	"objects" : [
		{
			"@objectName" : "CISCO-IETF-FRR-MIB::cmplsFrrProtected",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when a tunnel running over an",
				"interface as specified in the mplsFrrConstTable is initially",
				"protected by the backup tunnel also specified in the",
				"cmplsFrrConstTable. This notification should not be generated",
				"for each subsequent tunnel that is backed up by the FRR feature",
				"on this LSR, as this may result in potential scaling issues",
				"with regard to LSR performance and network loading. Note also",
				"that notifications MUST be generated in accordance with the",
				"cmplsFrrNotifMaxRate."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cmplsFrrProtected",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "There are $v1 tunnel(s) protecting this interface and it is protecting $v2 tunnel(s)"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-IETF-FRR-MIB::cmplsFrrProtected CISCO-IETF-FRR-MIB::cmplsFrrConstNumProtectingTunOnIf u 9999 CISCO-IETF-FRR-MIB::cmplsFrrConstNumProtectedTunOnIf u 9999 CISCO-IETF-FRR-MIB::cmplsFrrConstBandwidth u 9999",
			"trap" : {
				"name" : "CISCO-IETF-FRR-MIB::cmplsFrrProtected",
				"oid" : "1.3.6.1.4.1.9.10.98.0.1",
