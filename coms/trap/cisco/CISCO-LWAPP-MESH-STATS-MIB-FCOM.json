	"objects" : [
		{
			"@objectName" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshQueueOverflow",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent by the agent when a",
				"queue overflows and as a result packets get dropped.",
				"After each time interval, defined by object ",
				"'clMeshNodeStatsTimeInterval', mesh node sends its ",
				"queue statistics to the controller which also includes ",
				"cumulative number of packets dropped, defined by object ",
				"'clMeshNodePktQueuePktsDropped'. If the packets dropped ",
				"is higher than the previous time notification generated ",
				"then this notification is raised.."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoLwappMeshQueueOverflow",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "$v1",
				"Summary" : "Queue has overflow with a peak of $v2 and is now dropping packets."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshQueueOverflow CISCO-LWAPP-AP-MIB::cLApName s EXAMPLE CISCO-LWAPP-MESH-STATS-MIB::clMeshNodePktQueuePeak u 9999 CISCO-LWAPP-MESH-STATS-MIB::clMeshNodePktQueuePktsDropped c 9999",
			"trap" : {
				"name" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshQueueOverflow",
				"oid" : "1.3.6.1.4.1.9.9.617.0.1",
				"variables" : [
					{
						"name" : "CISCO-LWAPP-AP-MIB::cLApName",
						"oid" : "1.3.6.1.4.1.9.9.513.1.1.1.1.5",
						"valueType" : "OCTET STRING"
					},
					{
						"name" : "CISCO-LWAPP-MESH-STATS-MIB::clMeshNodePktQueuePeak",
						"oid" : "1.3.6.1.4.1.9.9.617.1.1.2.1.3",
						"valueType" : "GAUGE"
					},
					{
						"name" : "CISCO-LWAPP-MESH-STATS-MIB::clMeshNodePktQueuePktsDropped",
						"oid" : "1.3.6.1.4.1.9.9.617.1.1.2.1.4",
						"valueType" : "COUNTER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshExcessiveAssociation",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent by the agent when cumulative",
				"association counter at parent mesh node exceeds value",
				"configured in 'clMeshNodeExcessiveAssociationThreshold'.",
				"cLApName is the name of the parent mesh node."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoLwappMeshExcessiveAssociation",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v1",
				"Summary" : "Cumulative associations counter exceeded threshold with $v2."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshExcessiveAssociation CISCO-LWAPP-AP-MIB::cLApName s EXAMPLE CISCO-LWAPP-MESH-STATS-MIB::clMeshNodeAssociationCount c 9999",
			"trap" : {
				"name" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshExcessiveAssociation",
				"oid" : "1.3.6.1.4.1.9.9.617.0.2",
				"variables" : [
					{
						"name" : "CISCO-LWAPP-AP-MIB::cLApName",
						"oid" : "1.3.6.1.4.1.9.9.513.1.1.1.1.5",
						"valueType" : "OCTET STRING"
					},
					{
						"name" : "CISCO-LWAPP-MESH-STATS-MIB::clMeshNodeAssociationCount",
						"oid" : "1.3.6.1.4.1.9.9.617.1.1.1.1.10",
						"valueType" : "COUNTER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshSecBackhaulChange",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent by the agent when the MAP identified by",
				"clApName intiates the backhaul change from primary to",
				"secondary. cLApName identifies the AP that receives the",
				"change. The MAC address of the initiating AP is sent as a part of the",
				"index of clMeshNodeBackhaul. clMeshNodeBackhaul identifies the",
				"secondary backhaul channel used. The MAC address of the receiving AP",
				"is sent as a part of the index of cLApName."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoLwappMeshSecBackhaulChange",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v2",
				"Summary" : "MAP from $v2 has initiated the backhaul change from primary to secondary."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshSecBackhaulChange CISCO-LWAPP-MESH-STATS-MIB::clMeshInitiatingApName s EXAMPLE CISCO-LWAPP-AP-MIB::cLApName s EXAMPLE CISCO-LWAPP-MESH-MIB::clMeshNodeBackhaul i 1 CISCO-LWAPP-MESH-STATS-MIB::clMeshNodeSecBackhaulCount c 9999",
			"trap" : {
				"name" : "CISCO-LWAPP-MESH-STATS-MIB::ciscoLwappMeshSecBackhaulChange",
				"oid" : "1.3.6.1.4.1.9.9.617.0.3",
				"variables" : [
					{
						"name" : "CISCO-LWAPP-MESH-STATS-MIB::clMeshInitiatingApName",
						"oid" : "1.3.6.1.4.1.9.9.617.1.3.1",
						"valueType" : "OCTET STRING"
					},
					{
						"name" : "CISCO-LWAPP-AP-MIB::cLApName",
						"oid" : "1.3.6.1.4.1.9.9.513.1.1.1.1.5",
						"valueType" : "OCTET STRING"
					},
					{
						"enums" : {
							"1" : "dot11a",
							"2" : "dot11b",
							"3" : "dot11g"
						},
						"name" : "CISCO-LWAPP-MESH-MIB::clMeshNodeBackhaul",
						"oid" : "1.3.6.1.4.1.9.9.616.1.1.1.1.3",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-LWAPP-MESH-STATS-MIB::clMeshNodeSecBackhaulCount",
						"oid" : "1.3.6.1.4.1.9.9.617.1.1.1.1.9",
						"valueType" : "COUNTER"
					}
				]
			}
		},
		{
