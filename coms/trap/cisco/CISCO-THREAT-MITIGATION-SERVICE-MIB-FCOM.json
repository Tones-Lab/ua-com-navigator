			"certification" : "STANDARD",
			"description" : [
				"This notification is generated by the consumer when",
				"consumer acts upon a particular threat and changes the state",
				"of the threat. ",
				"This notification will be generated only when notification",
				"generation is enabled for the corresponding TIDP group",
				"through ciTmsGroupNotifEnable."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoTmsThreatStatusChange",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Consumer has changed threat state to $v2 with priority $v3"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsThreatStatusChange CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatVer u 9999 CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatStatus i 1 CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatPriority u 9999",
