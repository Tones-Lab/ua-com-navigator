		{
			"@objectName" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsConsStateChange",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated to indicate the current",
				"operational state of the consumer, when the consumer",
				"undergoes a state change."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoTmsConsStateChange",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Consumer state is currently $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsConsStateChange CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsConsumerState i 1",
			"trap" : {
				"name" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsConsStateChange",
				"oid" : "1.3.6.1.4.1.9.9.603.0.1",
				"variables" : [
					{
						"enums" : {
							"1" : "disabled",
							"2" : "enabled"
						},
						"name" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsConsumerState",
						"oid" : "1.3.6.1.4.1.9.9.603.1.1.8",
						"valueType" : "INTEGER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsControllerUnreachable",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated by the consumer when the",
				"controller it has registered with becomes unreachable.",
				"This notification will be generated only when notification",
				"generation is enabled for the corresponding TIDP group",
				"through ciTmsGroupNotifEnable."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 3) ? 1 : (($v1 == 1) ? 2 : 3)"
				},
				"EventType" : "ciscoTmsControllerUnreachable",
				"ExpireTime" : {
					"eval" : "($v1 == 3) ? 3600 : (($v1 == 1) ? 86400 : 3600)"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 3 : (($v1 == 3) ? 0 : 2)"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1 >= 2 || $v1 == 4) ? 'Consumer is currently in $v1 to an unreachable controller' : 'Consumer has registered with controller'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsControllerUnreachable CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsGroupConsumerRegStatus i 1",
			"trap" : {
				"name" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsControllerUnreachable",
				"oid" : "1.3.6.1.4.1.9.9.603.0.2",
				"variables" : [
					{
						"enums" : {
							"1" : "notRegistered",
							"2" : "registrationRequestSent",
							"3" : "registered",
							"4" : "registrationFailed"
						},
						"name" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsGroupConsumerRegStatus",
						"oid" : "1.3.6.1.4.1.9.9.603.1.2.1.1.4",
						"valueType" : "INTEGER"
					}
				]
			}
		},
		{
