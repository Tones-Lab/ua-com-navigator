		{
			"@objectName" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsMitigationActionFailed",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated by the consumer when the",
				"mitigation action enforced for a particular threat fails.",
				"The notification contains the information about the failed",
				"mitigation action and the reason for the failure indicated",
				"by ciTmsThreatActionFailReason.",
				"This notification will be generated only when notification",
				"generation is enabled for the corresponding TIDP group",
				"through ciTmsGroupNotifEnable."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoTmsMitigationActionFailed",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v4 ne '') ? 'Mitigation action for threat has failed due to $v4' : 'Mitigation action for threat has failed'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsMitigationActionFailed CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatActionParamType i 1 CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatActionParamLength u 9999 CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatActionParamValue s EXAMPLE CISCO-THREAT-MITIGATION-SERVICE-MIB::ciTmsThreatActionFailReason s EXAMPLE",
			"trap" : {
				"name" : "CISCO-THREAT-MITIGATION-SERVICE-MIB::ciscoTmsMitigationActionFailed",
				"oid" : "1.3.6.1.4.1.9.9.603.0.4",
				"variables" : [
