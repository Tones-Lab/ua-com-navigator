		{
			"@objectName" : "CISCO-SRST-MIB::csrstStateChange",
			"certification" : "STANDARD",
			"description" : [
				"An SRST up or down state change notification",
				"is generated. This indicates one or more phones ",
				"is registered to the SRST router or none is",
				"registered. ",
				""
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v2 == 2) ? 2 : 1"
				},
				"EventType" : "csrstStateChange",
				"ExpireTime" : {
					"eval" : "($v2 == 2) ? 86400 : 3600"
				},
				"Severity" : {
					"eval" : "($v1 == 2) ? 3 : (($v1 == 3) ? 4 : 0)"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v3 ne '') ? 'SRST status is $v2 due to $v3.' : 'SRST status is $v2'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-SRST-MIB::csrstStateChange CISCO-SRST-MIB::csrstSysNotifSeverity i 1 CISCO-SRST-MIB::csrstState i 1 CISCO-SRST-MIB::csrstSysNotifReason s EXAMPLE",
			"trap" : {
				"name" : "CISCO-SRST-MIB::csrstStateChange",
				"oid" : "1.3.6.1.4.1.9.9.441.0.1",
				"variables" : [
					{
						"enums" : {
							"1" : "clear",
							"2" : "minor",
							"3" : "major"
						},
						"name" : "CISCO-SRST-MIB::csrstSysNotifSeverity",
						"oid" : "1.3.6.1.4.1.9.9.441.2.2.2.1",
						"valueType" : "INTEGER"
					},
					{
						"enums" : {
							"1" : "active",
							"2" : "inactive"
						},
						"name" : "CISCO-SRST-MIB::csrstState",
						"oid" : "1.3.6.1.4.1.9.9.441.1.3.1",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-SRST-MIB::csrstSysNotifReason",
						"oid" : "1.3.6.1.4.1.9.9.441.2.2.2.2",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
