		{
			"@objectName" : "CISCO-SRST-MIB::csrstSipPhoneUnRegThresholdExceed",
			"certification" : "STANDARD",
			"description" : [
				"A SIP phone unregistration notification is",
				"generated when the number of SIP phone",
				"unregistrations have exceeded the threshold.",
				"The number of currently registered SIP ",
				"phones is provided here by ",
				"csrstSipPhoneCurrentRegistered object",
				"as a reference such that if ",
				"csrstSipPhoneCurrentRegistered falls below ",
				"csrstSipPhoneUnRegThreshold",
				"a notification will be generated to indicate",
				"that the number of unregistered SIP phones",
				"has crossed the threshold.",
				""
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "csrstSipPhoneUnRegThresholdExceed",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "SIP phone unregistrations have exceeded $v1 with currently having $v2."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-SRST-MIB::csrstSipPhoneUnRegThresholdExceed CISCO-SRST-MIB::csrstSipPhoneUnRegThreshold u 9999 CISCO-SRST-MIB::csrstSipPhoneCurrentRegistered u 9999",
			"trap" : {
				"name" : "CISCO-SRST-MIB::csrstSipPhoneUnRegThresholdExceed",
				"oid" : "1.3.6.1.4.1.9.9.441.0.3",
				"variables" : [
					{
						"name" : "CISCO-SRST-MIB::csrstSipPhoneUnRegThreshold",
						"oid" : "1.3.6.1.4.1.9.9.441.1.2.9",
						"valueType" : "UNSIGNED32"
					},
					{
						"name" : "CISCO-SRST-MIB::csrstSipPhoneCurrentRegistered",
						"oid" : "1.3.6.1.4.1.9.9.441.1.3.2",
						"valueType" : "GAUGE"
					}
				]
			}
		},
