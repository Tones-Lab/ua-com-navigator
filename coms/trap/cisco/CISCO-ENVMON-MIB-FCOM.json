		{
			"@objectName" : "CISCO-ENVMON-MIB::ciscoEnvMonShutdownNotification",
			"certification" : "STANDARD",
			"description" : [
				"A ciscoEnvMonShutdownNotification is sent if the environmental",
				"monitor detects a testpoint reaching a critical state",
				"and is about to initiate a shutdown.  This notification",
				"contains no objects so that it may be encoded and sent in the",
				"shortest amount of time possible.  Even so, management",
				"applications should not rely on receiving such a notification",
				"as it may not be sent before the shutdown completes."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoEnvMonShutdownNotification",
				"ExpireTime" : 3600,
				"Severity" : 5,
				"SubNode" : "Device",
				"Summary" : "Critical state detected - shutdown initiating"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-ENVMON-MIB::ciscoEnvMonShutdownNotification",
			"trap" : {
				"name" : "CISCO-ENVMON-MIB::ciscoEnvMonShutdownNotification",
				"oid" : "1.3.6.1.4.1.9.9.13.3.0.1",
				"variables" : []
			}
		},
