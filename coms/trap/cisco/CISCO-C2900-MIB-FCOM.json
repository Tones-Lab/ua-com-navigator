	"objects" : [
		{
			"@objectName" : "CISCO-C2900-MIB::c2900AddressViolation",
			"certification" : "STANDARD",
			"description" : [
				"The addressViolation notification is generated",
				"when an address violation is detected",
				"on a secured port. The generation of the",
				"addressViolation notification can be enabled",
				"or suppressed using the object",
				"c2900ConfigAddressViolationAction.",
				"The particular secured port is indicated by",
				"the value of c2900PortIfIndex."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "c2900AddressViolation",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "ifIndex: $v1",
				"Summary" : "AddressViolation notification generated for $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-C2900-MIB::c2900AddressViolation CISCO-C2900-MIB::c2900PortIfIndex i 9999",
			"trap" : {
				"name" : "CISCO-C2900-MIB::c2900AddressViolation",
				"oid" : "1.3.6.1.4.1.9.9.87.2.0.1",
				"variables" : [
					{
						"name" : "CISCO-C2900-MIB::c2900PortIfIndex",
						"oid" : "1.3.6.1.4.1.9.9.87.1.4.1.1.25",
						"valueType" : "INTEGER32"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-C2900-MIB::c2900BroadcastStorm",
			"certification" : "STANDARD",
			"description" : [
				"The broadcastStorm notification is generated",
				"upon detecting a port is receiving",
				"broadcast packets at a rate crossing",
				"the specified broadcast threshold.",
				"This trap is only for the rising threshold.",
				"The particular port is indicated by the values of",
				"c2900PortModuleIndex and c2900PortIndex, and the",
				"value of the threshold is given by",
				"c2900PortBroadcastRisingThreshold."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "c2900BroadcastStorm",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "BroadcastStorm notification with rising threshold of $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-C2900-MIB::c2900BroadcastStorm CISCO-C2900-MIB::c2900PortBroadcastRisingThreshold u 9999",
			"trap" : {
				"name" : "CISCO-C2900-MIB::c2900BroadcastStorm",
				"oid" : "1.3.6.1.4.1.9.9.87.2.0.2",
				"variables" : [
					{
						"name" : "CISCO-C2900-MIB::c2900PortBroadcastRisingThreshold",
						"oid" : "1.3.6.1.4.1.9.9.87.1.4.1.1.20",
						"valueType" : "GAUGE"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-C2900-MIB::c2900RpsFailed",
			"certification" : "STANDARD",
			"description" : [
				"A redundant power system (RPS) is connected to the",
				"switch. The RpsFailed notification is generated",
				"upon detecting RPS failure."
