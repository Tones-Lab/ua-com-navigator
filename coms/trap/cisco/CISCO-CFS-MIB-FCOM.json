			"certification" : "STANDARD",
			"description" : [
				"Notification to covey the information of the action",
				"performed on a CFS capable feature.",
				"This notification gets generated when :",
				"- a CFS operation action is triggered on CFS capable",
				"feature",
				"- the result of last action performed on the CFS",
				"capable feature changes.",
				"The 'cfsFeatureOpLastActionResult' indicates the new",
				"value of the last action result performed on the feature.",
				""
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoCFSFeatureActionNotif",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "($v4 == 3) ? 3 : 2"
				},
				"SubNode" : "Device",
				"Summary" : "Action $v1 has a status of $v4"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CFS-MIB::ciscoCFSFeatureActionNotif CISCO-CFS-MIB::cfsFeatureOpLastAction i 1 CISCO-CFS-MIB::cfsFeatureOpLastScopeType i 1 CISCO-CFS-MIB::cfsFeatureOpLastScopeVal s EXAMPLE CISCO-CFS-MIB::cfsFeatureOpLastActionResult i 1",
			"trap" : {
				"name" : "CISCO-CFS-MIB::ciscoCFSFeatureActionNotif",
				"oid" : "1.3.6.1.4.1.9.9.433.0.1",
