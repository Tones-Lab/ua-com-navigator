		{
			"@objectName" : "CISCO-CIPCMPC-MIB::cipCmpcDirectionMismatch",
			"certification" : "STANDARD",
			"description" : [
				"This trap indicates that the ",
				"configured direction for the specified",
				"CMPC subchannel does not match",
				"the direction specified by the host.",
				"The CMPC subchannel will not activate",
				"as long as this mismatch exists."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cipCmpcDirectionMismatch",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "CMPC subchannel direction does not match that specified by the host ($v1)"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CIPCMPC-MIB::cipCmpcDirectionMismatch CISCO-CIPCMPC-MIB::cipCmpcSubChannelAdminDirection i 1",
			"trap" : {
				"name" : "CISCO-CIPCMPC-MIB::cipCmpcDirectionMismatch",
