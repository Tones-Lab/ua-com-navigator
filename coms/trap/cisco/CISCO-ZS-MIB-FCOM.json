		{
			"@objectName" : "CISCO-ZS-MIB::zoneMergeSuccessNotify",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated whenever a zone",
				"merge happens successfully.",
				"If all VSANs on a link have a zone-merge success",
				"at the same time, then just one notification is",
				"generated in which zoneMergeSuccessVSANNum object",
				"has a zero value."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 1,
				"EventType" : "zoneMergeNotify",
				"ExpireTime" : 3600,
				"Severity" : 0,
				"SubNode" : "ifIndex: $v1",
				"Summary" : {
					"eval" : "( $v2 == 0 ) ? 'All VSANs on the link have successfully zone-merged.' : 'VSAN number $v2 has successfully zone-merged.' "
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-ZS-MIB::zoneMergeSuccessNotify IF-MIB::ifIndex i 9999 CISCO-ZS-MIB::zoneMergeSuccessVSANNum u 9999",
