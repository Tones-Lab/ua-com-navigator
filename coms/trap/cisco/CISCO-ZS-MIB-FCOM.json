			"event" : {
				"EventCategory" : 3,
				"EventType" : "zoneActivateNotify",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "( $v1 == 2 ) ? 3 : (( $v1 == 4 ) ? 3 : 2 )"
				},
				"SubNode" : "Device",
				"Summary" : "The Zone ( $v2 ) has had an active status change on a VSAN, new status is $v1."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
