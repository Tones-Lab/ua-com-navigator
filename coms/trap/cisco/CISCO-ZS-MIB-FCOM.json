		{
			"@objectName" : "CISCO-ZS-MIB::zoneUnsuppMemInIntOpModeNotify",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated whenever one or",
				"more zone members are not supported in the",
				"mode in which the local switch is currently",
				"configured to function. The switch can be",
				"configured to operate in certain 'interoperable'",
				"modes on a VSAN basis to enable it to work with ",
				"switches from other vendors. The zoneVsanId object ",
				"identifies the VSAN on which the mismatch has",
				"been identified."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "zoneUnsuppMemInIntOpModeNotify",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "One or more members of the zone are not supported by the current zone configuration."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-ZS-MIB::zoneUnsuppMemInIntOpModeNotify CISCO-ZS-MIB::zoneVsanId i 999",
			"trap" : {
