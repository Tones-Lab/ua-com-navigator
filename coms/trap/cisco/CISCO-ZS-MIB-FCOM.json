		{
			"@objectName" : "CISCO-ZS-MIB::zoneServiceReqRejNotify",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated whenever the local ",
				"Zone Server rejects a registration request.",
				"The Zone Server should update the ",
				"zoneServiceRejReasonCode and zoneServiceRejReason-",
				"CodeExp objects with the corresponding reason ",
				"code and reason code explanation before sending the ",
				"notification.",
				"The zoneMemberType and zoneMemberID identify the ",
				"source of the request."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "zoneServiceReqRejNotify",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "The Device with ID: $v2 has been denied registration to the Zone Server with code: $v3, reason: $v4"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-ZS-MIB::zoneServiceReqRejNotify CISCO-ZS-MIB::zoneMemberFormat i 1 CISCO-ZS-MIB::zoneMemberID s EXAMPLE CISCO-ZS-MIB::zoneServiceRejReasonCode i 1 CISCO-ZS-MIB::zoneServiceRejReasonCodeExp i 1",
			"trap" : {
				"name" : "CISCO-ZS-MIB::zoneServiceReqRejNotify",
				"oid" : "1.3.6.1.4.1.9.9.294.1.4.0.1",
				"variables" : [
					{
						"enums" : {
							"1" : "wwn",
							"2" : "domainPort",
							"3" : "fcAddr",
							"4" : "fwwn",
							"5" : "symNodeName",
							"6" : "intf",
