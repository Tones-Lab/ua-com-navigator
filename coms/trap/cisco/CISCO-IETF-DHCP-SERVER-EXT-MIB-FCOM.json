		{
			"@objectName" : "CISCO-IETF-DHCP-SERVER-EXT-MIB::cDhcpv4ServerIfLeaseLimitExceeded",
			"certification" : "STANDARD",
			"description" : [
				"This notification indicates when a client has attempted to obtain",
				"a lease and the lease was not granted because the number of leases ",
				"granted to clients on the receiving interface is equal to ",
				"cDhcpv4ServerIfLeaseLimit."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cDhcpv4ServerIfLeaseLimitExceeded",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v1",
				"Summary" : "DHCP has reached it's lease limit of $v3 for client $v2 on interface $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-IETF-DHCP-SERVER-EXT-MIB::cDhcpv4ServerIfLeaseLimitExceeded IF-MIB::ifName s EXAMPLE CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerClientPhysicalAddress s EXAMPLEEXAMPLEEXAM CISCO-IETF-DHCP-SERVER-EXT-MIB::cDhcpv4ServerIfLeaseLimit u 9999",
			"trap" : {
				"name" : "CISCO-IETF-DHCP-SERVER-EXT-MIB::cDhcpv4ServerIfLeaseLimitExceeded",
				"oid" : "1.3.6.1.4.1.9.10.122.0.2.0.1",
				"variables" : [
					{
