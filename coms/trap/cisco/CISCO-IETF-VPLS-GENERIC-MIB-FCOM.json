			"certification" : "STANDARD",
			"description" : [
				"The cvplsStatusChanged notification is generated",
				"when there is a change in the administrative or",
				"operating status of a VPLS service."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v2 == 1) ? 1 : (($v2 == 2) ? 2 : 3)"
				},
				"EventType" : "cvplsStatusChanged",
				"ExpireTime" : {
					"eval" : "($v2 == 1) ? 3600 : (($v2 == 2) ? 86400 : 3600)"
				},
				"Severity" : {
					"eval" : "($v2 == 1) ? 0 : (($v2 == 2) ? 4 : 3)"
				},
				"SubNode" : "$v1",
				"Summary" : "VPLS service $v1 has changed state to admin: $v2, oper: $v3"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
