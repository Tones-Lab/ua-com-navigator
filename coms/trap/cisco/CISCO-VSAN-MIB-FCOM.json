			"certification" : "STANDARD",
			"description" : [
				"A state change notification is generated whenever",
				"vsanOperState is changed. The index and both states of ",
				"the VSAN after the change, are included as variables ",
				"in the notification."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v3 == 1) ? 1 : 2"
				},
				"EventType" : "vsanStatusChange",
				"ExpireTime" : {
					"eval" : "($v3 == 1) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v3 == 1) ? 0 : 3"
				},
				"SubNode" : "VsanIndex: $v1",
				"Summary" : "VsanIndex $v1 is now Admin $v2 and Oper $v3"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-VSAN-MIB::vsanStatusChange CISCO-VSAN-MIB::notifyVsanIndex i 999 CISCO-VSAN-MIB::vsanAdminState i 1 CISCO-VSAN-MIB::vsanOperState i 1",
