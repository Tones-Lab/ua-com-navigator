			"@objectName" : "CISCO-BITS-CLOCK-MIB::ciscoBitsClockFreerun",
			"certification" : "STANDARD",
			"description" : [
				"An external clock source is unavailable. The internal ",
				"clock will operate in freerun mode using appropiate ",
				"local oscillator. Therefore, it does not provide ",
				"synchronous clocking. This is the least stable of ",
				"all operating modes. "
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ciscoBitsClockFreerun",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "$v1",
				"Summary" : "An external clock source $v1 is unavailable, running in freerun mode."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-BITS-CLOCK-MIB::ciscoBitsClockFreerun ENTITY-MIB::entPhysicalDescr s EXAMPLE",
			"trap" : {
				"name" : "CISCO-BITS-CLOCK-MIB::ciscoBitsClockFreerun",
