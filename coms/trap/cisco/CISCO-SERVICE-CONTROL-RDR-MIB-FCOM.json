			"certification" : "STANDARD",
			"description" : [
				"The system generates this notification to indicate that the",
				"system has either encountered congestion or unavailibility of",
				"the RDR collector with the category identified by the value of",
				"cscRdrCategoryID and has started discarding reports destined to",
				"this collector. The value of cscRdrCategoryNumReportsDiscarded",
				"indicates the number of discarded reports."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 2,
				"EventType" : "cscRdrCategoryDiscardingReportsTrap",
				"ExpireTime" : 86400,
				"Severity" : 3,
				"SubNode" : "$v1 : RDR CatID: $v2",
				"Summary" : "System has detected unavailibility of RDR collector $v2"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryDiscardingReportsTrap ENTITY-MIB::entPhysicalName s EXAMPLE CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryID u 9999",
			"trap" : {
				"name" : "CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryDiscardingReportsTrap",
