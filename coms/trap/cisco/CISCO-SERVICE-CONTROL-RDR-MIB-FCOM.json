		{
			"@objectName" : "CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryStoppedDiscardingReportsTrap",
			"certification" : "STANDARD",
			"description" : [
				"The system generates this notification to indicate that the",
				"system has recovered from congestion or unavailibility of the",
				"RDR collector identified by the value of cscRdrCategoryID and",
				"has stopped discarding reports being sent to this RDR",
				"collector."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 1,
				"EventType" : "cscRdrCategoryDiscardingReportsTrap",
				"ExpireTime" : 3600,
				"Severity" : 0,
				"SubNode" : "$v1 : RDR CatID: $v2",
				"Summary" : "System recovered from RDR collector $v2 unavailibility"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryStoppedDiscardingReportsTrap ENTITY-MIB::entPhysicalName s EXAMPLE CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryID u 9999",
			"trap" : {
				"name" : "CISCO-SERVICE-CONTROL-RDR-MIB::cscRdrCategoryStoppedDiscardingReportsTrap",
				"oid" : "1.3.6.1.4.1.9.9.637.0.1",
				"variables" : [
					{
						"name" : "ENTITY-MIB::entPhysicalName",
