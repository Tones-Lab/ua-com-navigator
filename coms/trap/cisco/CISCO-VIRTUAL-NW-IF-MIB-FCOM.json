		{
			"@objectName" : "CISCO-VIRTUAL-NW-IF-MIB::virtualNwIfCreateEntryNotify",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated by the agent",
				"whenever an virtual interface is created in the",
				"'virtualNwIfTable'. Note that this notification",
				"is generated only when a new entry is added to the ",
				"table and not for any modifications to an existing",
				"entry. "
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "virtualNwIfCreateEntryNotify",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v2:$v1",
				"Summary" : "Virtual interface $v2:$v1 has been created"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-VIRTUAL-NW-IF-MIB::virtualNwIfCreateEntryNotify CISCO-VIRTUAL-NW-IF-MIB::virtualNwIfIndex i 9999 IF-MIB::ifName s EXAMPLE",
			"trap" : {
				"name" : "CISCO-VIRTUAL-NW-IF-MIB::virtualNwIfCreateEntryNotify",
				"oid" : "1.3.6.1.4.1.9.9.290.1.3.0.1",
				"variables" : [
					{
						"name" : "CISCO-VIRTUAL-NW-IF-MIB::virtualNwIfIndex",
