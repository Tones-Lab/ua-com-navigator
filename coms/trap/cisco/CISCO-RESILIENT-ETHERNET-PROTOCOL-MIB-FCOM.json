		{
			"@objectName" : "CISCO-RESILIENT-ETHERNET-PROTOCOL-MIB::crepLinkStatus",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent when a REP interface has entered or",
				"left REP link operational status. The link is considered ",
				"operational when 'crepIfOperStatus' is 'twoWay'. ",
				"'crepIfOperStatus' would be 'none' if the ",
				"crepInterfaceConfigEntry entry has been removed."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "(($v2 >= 2  && $v2 <= 4) || ($v2 == 6 || $v2 == 7)) ? 2 : (($v2 == 5) ? 1 : 3)"
				},
				"EventType" : "crepLinkStatus",
				"ExpireTime" : {
					"eval" : "(($v2 >= 2  && $v2 <= 4) || ($v2 == 6 || $v2 == 7)) ? 86400 : (($v2 == 5) ? 3600 : 3600)"
				},
				"Severity" : {
					"eval" : "(($v2 >= 2  && $v2 <= 4) || ($v2 == 6 || $v2 == 7)) ? 3 : (($v2 == 5) ? 0 : 2)"
				},
				"SubNode" : "Device",
				"Summary" : "REP link status is currently $v2"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-RESILIENT-ETHERNET-PROTOCOL-MIB::crepLinkStatus CISCO-RESILIENT-ETHERNET-PROTOCOL-MIB::crepIfSegmentId u 9999 CISCO-RESILIENT-ETHERNET-PROTOCOL-MIB::crepIfOperStatus i 1",
