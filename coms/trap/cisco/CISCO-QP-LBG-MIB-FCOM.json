			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cqlQamOverSubscribedAlert",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v1",
				"Summary" : {
					"eval" : "( $v2 == 1 ) ? 'The qam has been over subscribed on interface $v1' : 'The qam is no longer over subscribed on interface $v1' "
