			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-QP-LBG-MIB::cqlQamOverSubscribedAlert IF-MIB::ifDescr s EXAMPLE CISCO-QP-LBG-MIB::cqlqpQamOversubscribedStatus i 1",
