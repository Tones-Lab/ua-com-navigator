			"@objectName" : "CISCO-QP-LBG-MIB::cqlQpStateChange",
			"certification" : "STANDARD",
			"description" : [
				"The device generates this notification when a QP state",
				"has changed and the value of cqlQpNotifyEnable is 'true'."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 3) ? 1 : 2"
				},
				"EventType" : "cqlQpStateChange",
				"ExpireTime" : {
					"eval" : "($v1 == 3) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 3 : (($v1 == 2) ? 2 : 3)"
				},
				"SubNode" : "Device",
				"Summary" : "QP state has changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-QP-LBG-MIB::cqlQpStateChange CISCO-QP-LBG-MIB::cqlqpState i 1",
			"trap" : {
				"name" : "CISCO-QP-LBG-MIB::cqlQpStateChange",
				"oid" : "1.3.6.1.4.1.9.9.824.0.1",
				"variables" : [
					{
						"enums" : {
							"1" : "inactive",
							"2" : "init",
							"3" : "active"
						},
						"name" : "CISCO-QP-LBG-MIB::cqlqpState",
						"oid" : "1.3.6.1.4.1.9.9.824.1.1.1.7",
						"valueType" : "INTEGER"
					}
				]
			}
		},
