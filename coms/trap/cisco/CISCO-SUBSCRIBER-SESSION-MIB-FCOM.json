		},
		{
			"@objectName" : "CISCO-SUBSCRIBER-SESSION-MIB::csubSessionDeltaPercentFallingThreshNotif",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when the value of",
				"csubAggStatsThreshNotifEnable is 'true' AND the value of",
				"csubSessionDeltaPercentFallingThresh is non-zero AND",
				"If the delta_percent value of the current",
				"evaluation interval is",
				"greater than the value of",
				"csubSessionDeltaPercentFallingThresh.",
				"The delta as a percentage of csubAggStatsUpSessions",
				"(delta_percent)",
				"is calculated as follows:",
				"current value of csubAggStatsUpSessions = value(n)",
				"previous sampled value of csubAggStatsUpSessions =",
				"value(n-1)",
				"delta_percent = ((value(n-1) - value(n)) /",
				"value(n-1)) * 100"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "csubSessionDeltaPercentFallingThreshNotif",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Session percentage is higher than $v2% with $v1 sessions active"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-SUBSCRIBER-SESSION-MIB::csubSessionDeltaPercentFallingThreshNotif CISCO-SUBSCRIBER-SESSION-MIB::csubAggStatsUpSessions u 9999 CISCO-SUBSCRIBER-SESSION-MIB::csubSessionDeltaPercentFallingThresh u 9999",
			"trap" : {
				"name" : "CISCO-SUBSCRIBER-SESSION-MIB::csubSessionDeltaPercentFallingThreshNotif",
				"oid" : "1.3.6.1.4.1.9.9.786.0.4",
				"variables" : [
					{
						"name" : "CISCO-SUBSCRIBER-SESSION-MIB::csubAggStatsUpSessions",
						"oid" : "1.3.6.1.4.1.9.9.786.1.2.1.1.5",
						"valueType" : "GAUGE"
					},
					{
						"name" : "CISCO-SUBSCRIBER-SESSION-MIB::csubSessionDeltaPercentFallingThresh",
						"oid" : "1.3.6.1.4.1.9.9.786.1.2.3.1.3",
						"valueType" : "UNSIGNED32"
					}
				]
			}
		}
	]
}