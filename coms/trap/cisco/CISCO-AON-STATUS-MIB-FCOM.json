		{
			"@objectName" : "CISCO-AON-STATUS-MIB::caonSendResponseThresholdExceeded",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent if the destination end point",
				"response time exceeds the threshold value specified in the ",
				"Send Bladelet.",
				"caonPepEndPointUrl identifies the URI of the end point the",
				"message was being delivered to.",
				"caonSendResponseThreshold identifies the end point response time",
				"threshold value configured in the Send bladelet."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "caonSendResponseThresholdExceeded",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "Message response from $v1 timed out"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-AON-STATUS-MIB::caonSendResponseThresholdExceeded CISCO-AON-STATUS-MIB::caonPepEndPointUrl s EXAMPLE CISCO-AON-STATUS-MIB::caonSendResponseThreshold t 31536000",
			"trap" : {
				"name" : "CISCO-AON-STATUS-MIB::caonSendResponseThresholdExceeded",
				"oid" : "1.3.6.1.4.1.9.9.646.0.5",
				"variables" : [
					{
						"name" : "CISCO-AON-STATUS-MIB::caonPepEndPointUrl",
