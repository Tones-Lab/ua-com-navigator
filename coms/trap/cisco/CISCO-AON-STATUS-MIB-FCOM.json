				"ExpireTime" : 3600,
				"Severity" : 1,
				"SubNode" : "Device",
				"Summary" : "New PEP $v1 deployed"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-AON-STATUS-MIB::caonNewPepDeployed CISCO-AON-STATUS-MIB::caonPepName s EXAMPLE",
			"trap" : {
