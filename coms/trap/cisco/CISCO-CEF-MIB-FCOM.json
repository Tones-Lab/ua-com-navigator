	"objects" : [
		{
			"@objectName" : "CISCO-CEF-MIB::cefResourceFailure",
			"certification" : "STANDARD",
			"description" : [
				"A cefResourceFailure notification is generated when ",
				"CEF resource failure on the managed entity is ",
				"detected. The reason for this failure is indicated ",
				"by cefResourcefFailureReason."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cefResourceFailure",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "CEF resource failure with an ErrorCode of $v1 detected"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CEF-MIB::cefResourceFailure CISCO-CEF-MIB::cefResourceFailureReason i 1",
			"trap" : {
				"name" : "CISCO-CEF-MIB::cefResourceFailure",
				"oid" : "1.3.6.1.4.1.9.9.492.0.1",
				"variables" : [
					{
						"enums" : {
							"1" : "none",
							"2" : "mallocFailure",
							"3" : "hwFailure",
							"4" : "keepaliveFailure",
							"5" : "noMsgBuffer",
							"6" : "invalidMsgSize",
							"7" : "internalError"
						},
						"name" : "CISCO-CEF-MIB::cefResourceFailureReason",
						"oid" : "1.3.6.1.4.1.9.9.492.1.4.2.1.2",
						"valueType" : "INTEGER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-CEF-MIB::cefPeerStateChange",
			"certification" : "STANDARD",
			"description" : [
				"A cefPeerStateChange notification is generated if",
				"change in cefPeerOperState is detected for the",
				"peer entity."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 2) ? 1 : 2"
				},
				"EventType" : "cefPeerStateChange",
				"ExpireTime" : {
					"eval" : "($v1 == 2) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 5 : (($v1 == 2) ? 0 : 2)"
				},
				"SubNode" : "Device",
				"Summary" : "CEF Peer state has changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CEF-MIB::cefPeerStateChange CISCO-CEF-MIB::cefPeerOperState i 1",
			"trap" : {
				"name" : "CISCO-CEF-MIB::cefPeerStateChange",
				"oid" : "1.3.6.1.4.1.9.9.492.0.2",
				"variables" : [
					{
						"enums" : {
							"1" : "peerDisabled",
							"2" : "peerUp",
							"3" : "peerHold"
						},
						"name" : "CISCO-CEF-MIB::cefPeerOperState",
						"oid" : "1.3.6.1.4.1.9.9.492.1.6.1.1.2",
						"valueType" : "INTEGER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-CEF-MIB::cefPeerFIBStateChange",
			"certification" : "STANDARD",
			"description" : [
				"A cefPeerFIBStateChange notification is generated if",
				"change in cefPeerFIBOperState is detected for the",
				"peer entity."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 1) ? 2 : (($v1 == 2) ? 1 : 3)"
				},
				"EventType" : "cefPeerFIBStateChange",
				"ExpireTime" : {
					"eval" : "($v1 == 1) ? 86400 : (($v1 == 2) ? 3600 : 3600)"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 5 : (($v1 == 2) ? 0 : 3)"
				},
				"SubNode" : "Device",
				"Summary" : "FIB Peer state has changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CEF-MIB::cefPeerFIBStateChange CISCO-CEF-MIB::cefPeerFIBOperState i 1",
			"trap" : {
				"name" : "CISCO-CEF-MIB::cefPeerFIBStateChange",
				"oid" : "1.3.6.1.4.1.9.9.492.0.3",
				"variables" : [
					{
						"enums" : {
							"1" : "peerFIBDown",
							"2" : "peerFIBUp",
							"3" : "peerFIBReloadRequest",
							"4" : "peerFIBReloading",
							"5" : "peerFIBSynced"
						},
						"name" : "CISCO-CEF-MIB::cefPeerFIBOperState",
						"oid" : "1.3.6.1.4.1.9.9.492.1.6.2.1.1",
						"valueType" : "INTEGER"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-CEF-MIB::cefInconsistencyDetection",
			"certification" : "STANDARD",
			"description" : [
				"A cefInconsistencyDetection notification is generated",
				"when CEF consistency checkers detects an inconsistent ",
				"prefix in one of the CEF forwarding databases.",
				"Note that the generation of cefInconsistencyDetection",
				"notifications is throttled by the agent, as specified",
				"by the 'cefNotifThrottlingInterval' object."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cefInconsistencyDetection",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "CEF inconsistency detected"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-CEF-MIB::cefInconsistencyDetection CISCO-CEF-MIB::entLastInconsistencyDetectTime t 31536000",
			"trap" : {
				"name" : "CISCO-CEF-MIB::cefInconsistencyDetection",
				"oid" : "1.3.6.1.4.1.9.9.492.0.4",
				"variables" : [
					{
						"name" : "CISCO-CEF-MIB::entLastInconsistencyDetectTime",
						"oid" : "1.3.6.1.4.1.9.9.492.1.7.4",
						"valueType" : "TICKS"
					}
				]
			}
		}
	]
}