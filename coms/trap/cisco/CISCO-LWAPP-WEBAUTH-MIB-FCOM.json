			"@objectName" : "CISCO-LWAPP-WEBAUTH-MIB::cLWAPortalUnreachNotify",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when portal server is ",
				"unreachable/recovered."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v3 == 2) ? 1 : 2"
				},
				"EventType" : "cLWAPortalUnreachNotify",
				"ExpireTime" : {
					"eval" : "($v3 == 2) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v3 == 2) ? 0 : 4"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v3 == 2) ? 'Portal server $v2 is now reachable.' : 'Portal server $v2 is unreachable.'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-LWAPP-WEBAUTH-MIB::cLWAPortalUnreachNotify CISCO-LWAPP-WEBAUTH-MIB::cLWAPortalServerAddrType i 0 CISCO-LWAPP-WEBAUTH-MIB::cLWAPortalServerIp s EXAMPLE CISCO-LWAPP-WEBAUTH-MIB::cLWAPortalUnreachSet i 1",
