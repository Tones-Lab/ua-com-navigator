	"objects" : [
		{
			"@objectName" : "CISCO-ENTITY-REDUNDANCY-MIB::ceRedunEventSwitchover",
			"certification" : "STANDARD",
			"description" : [
				"A ceRedunEventSwitchover notification is sent when the",
				"ceRedunMbrProtectingMbr object changes value for a ",
				"secondary member. The objects should correspond to ",
				"the secondary member which changed its status.",
				"The objects should reflect the status following the ",
				"switchover. ",
				""
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "ceRedunEventSwitchover",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Redundancy secondary member has changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-ENTITY-REDUNDANCY-MIB::ceRedunEventSwitchover CISCO-ENTITY-REDUNDANCY-MIB::ceRedunMbrProtectingMbr u 9999 CISCO-ENTITY-REDUNDANCY-MIB::ceRedunMbrStatusCurrent b 1",
			"trap" : {
				"name" : "CISCO-ENTITY-REDUNDANCY-MIB::ceRedunEventSwitchover",
				"oid" : "1.3.6.1.4.1.9.9.498.0.1",
