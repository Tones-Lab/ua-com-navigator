		{
			"@objectName" : "CISCO-IF-THRESHOLD-MIB::cifthIfThresholdFired",
			"certification" : "STANDARD",
			"description" : [
				"The agent generates this notification when a threshold defined",
				"in a template is asserted on an interface.",
				"If a threshold is asserted for a given interface and template",
				"cifthTemplateNotifyHoldDownTime is set to 'holdDownTimer' for",
				"that template, and this notification has recently been sent for",
				"the same threshold on the same interface, then this",
				"notification may be suppressed."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 2,
				"EventType" : "cifthIfThreshold",
				"ExpireTime" : 86400,
				"Severity" : 4,
				"SubNode" : "Device",
				"Summary" : "Interface threshold asserted with severity $v3"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-IF-THRESHOLD-MIB::cifthIfThresholdFired CISCO-IF-THRESHOLD-MIB::cifthIfLastThresholdFired u 9999 CISCO-IF-THRESHOLD-MIB::cifthIfThresholdFiredLstChange t 36000 CISCO-IF-THRESHOLD-MIB::cifthIfThresholdFiredLstSeverity i 1",
			"trap" : {
