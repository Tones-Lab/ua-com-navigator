			}
		},
		{
			"@objectName" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacThresholdExceedNotif",
			"certification" : "STANDARD",
			"description" : [
				"cmnMacThresholdExceedNotif is sent when ",
				"cmnUtilizationUtilization exceeds or equals to the ",
				"cmnMACThresholdLimit for a given entPhysicalIndex.",
				"cmnMacThresholdExceedNotif is not sent when ",
				"cmnMACThresholdLimit is set to zero"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v2 == 0) ? 1 : 2"
				},
				"EventType" : "cmnMacThresholdExceedNotif",
				"ExpireTime" : {
					"eval" : "($v2 == 0) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v2 == 0) ? 0 : 3"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v2 == 0) ? 'Utilization of the forwarding table has gone back to 0' : 'Utilization of the forwarding table has risen to $v2'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-MAC-NOTIFICATION-MIB::cmnMacThresholdExceedNotif CISCO-MAC-NOTIFICATION-MIB::cmnUtilizationUtilization i 99 CISCO-MAC-NOTIFICATION-MIB::cmnMACThresholdLimit i 99 CISCO-MAC-NOTIFICATION-MIB::cmnUtilizationTimeStamp t 9999",
			"trap" : {
				"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacThresholdExceedNotif",
				"oid" : "1.3.6.1.4.1.9.9.215.2.0.3",
				"variables" : [
					{
						"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnUtilizationUtilization",
						"oid" : "1.3.6.1.4.1.9.9.215.1.4.5.1.2",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnMACThresholdLimit",
						"oid" : "1.3.6.1.4.1.9.9.215.1.4.2",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnUtilizationTimeStamp",
						"oid" : "1.3.6.1.4.1.9.9.215.1.4.5.1.3",
