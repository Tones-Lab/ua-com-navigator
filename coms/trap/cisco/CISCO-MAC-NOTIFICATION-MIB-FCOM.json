{
	"metaData" : {
		"sourceRevision" : "200706110000Z"
	},
	"objects" : [
		{
			"@objectName" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacChangedNotification",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when there is enough MAC",
				"address information to fully occupy a maximum size SNMP trap",
				"message. This notification is also generated when there",
				"is at least one MAC address changed or removed and the amount",
				"of time elapsed from the previous notification is greater",
				"than the maximum wait time denoted by ",
				"cmnNotificationInterval object. ",
				"If there are more MAC addresses information than can fit into",
				"one cmmHistTrapContent object, then multiple notifications",
				"will be generated. "
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cmnMacChangedNotification",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "MAC address has been modified : $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-MAC-NOTIFICATION-MIB::cmnMacChangedNotification CISCO-MAC-NOTIFICATION-MIB::cmnHistMacChangedMsg s EXAMPLE CISCO-MAC-NOTIFICATION-MIB::cmnHistTimestamp t 9999",
			"trap" : {
				"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacChangedNotification",
				"oid" : "1.3.6.1.4.1.9.9.215.2.0.1",
				"variables" : [
					{
						"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnHistMacChangedMsg",
						"oid" : "1.3.6.1.4.1.9.9.215.1.1.8.1.2",
						"valueType" : "OCTET STRING"
					},
					{
						"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnHistTimestamp",
						"oid" : "1.3.6.1.4.1.9.9.215.1.1.8.1.3",
						"valueType" : "TICKS"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacMoveNotification",
			"certification" : "STANDARD",
			"description" : [
				"cmnMacMoveNotification is generated when a MAC address is ",
				"moved between two interfaces."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cmnMacMoveNotification",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "$v1",
				"Summary" : "MAC address $v1 address has moved from PortID = $v3 to $v4"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-MAC-NOTIFICATION-MIB::cmnMacMoveNotification CISCO-MAC-NOTIFICATION-MIB::cmnMACMoveAddress s 01:01:01:01:01:01 CISCO-MAC-NOTIFICATION-MIB::cmnMACMoveVlanNumber i 999 CISCO-MAC-NOTIFICATION-MIB::cmnMACMoveFromPortId i 9999 CISCO-MAC-NOTIFICATION-MIB::cmnMACMoveToPortId i 9999 CISCO-MAC-NOTIFICATION-MIB::cmnMACMoveTime t 9999",
			"trap" : {
				"name" : "CISCO-MAC-NOTIFICATION-MIB::cmnMacMoveNotification",
				"oid" : "1.3.6.1.4.1.9.9.215.2.0.2",
				"variables" : [
					{
