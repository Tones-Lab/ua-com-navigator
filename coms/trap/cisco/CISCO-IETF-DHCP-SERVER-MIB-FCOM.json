		{
			"@objectName" : "CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerDuplicateAddress",
			"certification" : "STANDARD",
			"description" : [
				"This notification signifies that a duplicate IPv4 address has",
				"been detected. The DHCP server can detect this condition ",
				"through the ping-before-offer mechanism. Alternatively, the ",
				"client may have sent a DHCPDECLINE back to the server; this is ",
				"assumed to be the result of the client detecting that the ",
				"address was in use. In either case, the DHCP server marks the ",
				"IPv4 address as unavailable for leasing to clients. The ",
				"cDhcpv4ServerNotifyClientOrServerDetected object indicates ",
				"whether the client or server detected this condition."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cDhcpv4ServerDuplicateAddress",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "Duplicate IPv4 address $v1 detected by $v3"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerDuplicateAddress CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerNotifyDuplicateIpAddr s EXAM CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerNotifyDuplicateMac s EXAMPLEEXAMPLEEXAM CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerNotifyClientOrServerDetected i 1",
			"trap" : {
				"name" : "CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerDuplicateAddress",
				"oid" : "1.3.6.1.4.1.9.10.102.0.2.0.5",
				"variables" : [
					{
						"name" : "CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerNotifyDuplicateIpAddr",
						"oid" : "1.3.6.1.4.1.9.10.102.1.7.1",
						"valueType" : "OCTET STRING"
					},
					{
						"name" : "CISCO-IETF-DHCP-SERVER-MIB::cDhcpv4ServerNotifyDuplicateMac",
						"oid" : "1.3.6.1.4.1.9.10.102.1.7.2",
