			"@objectName" : "CISCO-LWAPP-SYS-MIB::ciscoLwappSysCpuUsageHigh",
			"certification" : "STANDARD",
			"description" : [
				"When a WLC detects its CPU usage is higher than the",
				"threshold configured in clsSysControllerCpuUsageThreshold",
				"this notification is generated with clsSysAlarmSet set ",
				"to true. When its CPU usage falls below the threshold",
				"lately, this notification is generated with",
				"clsSysAlarmSet set to false."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v2 == 2) ? 1 : 2"
				},
				"EventType" : "ciscoLwappSysCpuUsageHigh",
				"ExpireTime" : {
					"eval" : "($v2 == 2) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v2 == 2) ? 0 : 4"
				},
				"SubNode" : "Device",
				"Summary" : "WLC detects CPU usage high with $v1% usage across all CPUs."
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-LWAPP-SYS-MIB::ciscoLwappSysCpuUsageHigh CISCO-LWAPP-SYS-MIB::clsSysCurrentCpuUsage u 9999 CISCO-LWAPP-SYS-MIB::clsSysAlarmSet i 1",
			"trap" : {
				"name" : "CISCO-LWAPP-SYS-MIB::ciscoLwappSysCpuUsageHigh",
				"oid" : "1.3.6.1.4.1.9.9.618.0.4",
				"variables" : [
					{
						"name" : "CISCO-LWAPP-SYS-MIB::clsSysCurrentCpuUsage",
