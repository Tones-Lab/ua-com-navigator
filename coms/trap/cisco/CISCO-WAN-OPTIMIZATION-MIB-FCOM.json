{
	"metaData" : {
		"sourceRevision" : "201605220000Z"
	},
	"objects" : [
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoTfoConnectionOverload",
			"certification" : "STANDARD",
			"description" : [
				"This notification indicates that the device has reached its",
				"limit of number of connections it can optimize. No new",
				"connections will be picked-up for optimization, while the device",
				"is in this state."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cwoTfoConnectionOverload",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "No new connections added for optimization due to connections exceeding $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoTfoConnectionOverload CISCO-WAN-OPTIMIZATION-MIB::cwoTfoStatsMaxActiveConn C 9999",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoTfoConnectionOverload",
				"oid" : "1.3.6.1.4.1.9.9.762.0.1",
				"variables" : [
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoTfoStatsMaxActiveConn",
						"oid" : "1.3.6.1.4.1.9.9.762.1.2.1.3",
						"valueType" : "COUNTER64"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoPeerOverload",
			"certification" : "STANDARD",
			"description" : [
				"This notification indicates the device has reached its limit of",
				"number of peer devices with which it can optimize connections.",
				"While the device is in this state, connections from new peer",
				"device will not be picked-up for optimization."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cwoPeerOverload",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "No new peer device connections added for optimization due to connections exceeding $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoPeerOverload CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralMaxActivePeers u 9999",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoPeerOverload",
				"oid" : "1.3.6.1.4.1.9.9.762.0.2",
				"variables" : [
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralMaxActivePeers",
						"oid" : "1.3.6.1.4.1.9.9.762.1.1.2",
						"valueType" : "UNSIGNED32"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOn",
			"certification" : "STANDARD",
			"description" : [
				"This notification indicates that the device has detected that",
				"the current CPU utilization (stored in",
				"cpmCPUTotalMonIntervalValue) is higher than the",
				"cwoGeneralCpuThrottleHigh. As a result the device has entered",
				"into a throttling mode due to which connections under",
				"optimization may experience slowdown. This notification is",
				"generated the moment it occurs after a time lapse of 30 minutes",
				"since the previous notification."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 2,
				"EventType" : "cwoCpuThrottlingStatus",
				"ExpireTime" : 86400,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "Throttling enabled - CPU utilization $v3% is higher than throttle threshold $v1%"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOn CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottleHigh u 9999 CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottlPeriod i 9999 CISCO-PROCESS-MIB::cpmCPUTotalMonIntervalValue u 9999",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOn",
				"oid" : "1.3.6.1.4.1.9.9.762.0.3",
				"variables" : [
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottleHigh",
						"oid" : "1.3.6.1.4.1.9.9.762.1.1.3",
						"valueType" : "UNSIGNED32"
					},
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottlPeriod",
						"oid" : "1.3.6.1.4.1.9.9.762.1.1.5",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-PROCESS-MIB::cpmCPUTotalMonIntervalValue",
						"oid" : "1.3.6.1.4.1.9.9.109.1.1.1.1.10",
						"valueType" : "GAUGE"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOff",
			"certification" : "STANDARD",
			"description" : [
				"This notification indicates that device has detected that the",
				"CPU utilization of device (stored in",
				"cpmCPUTotalMonIntervalValue) has fallen below the",
				"cwoGeneralCpuThrottleLow from cwoGeneralCpuThrottleHigh. As a",
				"result the device has come out of the throttling mode and there",
				"should not be any negative impact of CPU usage on connection",
				"under optimization. This notification is generated the moment it",
				"occurs after a time lapse of 30 minutes since the previous",
				"notification."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 1,
				"EventType" : "cwoCpuThrottlingStatus",
				"ExpireTime" : 3600,
				"Severity" : 0,
				"SubNode" : "Device",
				"Summary" : "Throttling disabled - CPU utilization $v3% has dropped lower than throttle threshold $v1%"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOff CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottleLow u 9999 CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottlPeriod i 9999 CISCO-PROCESS-MIB::cpmCPUTotalMonIntervalValue u 9999",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoCpuThrottlingOff",
				"oid" : "1.3.6.1.4.1.9.9.762.0.4",
				"variables" : [
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottleLow",
						"oid" : "1.3.6.1.4.1.9.9.762.1.1.4",
						"valueType" : "UNSIGNED32"
					},
					{
						"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoGeneralCpuThrottlPeriod",
						"oid" : "1.3.6.1.4.1.9.9.762.1.1.5",
						"valueType" : "INTEGER"
					},
					{
						"name" : "CISCO-PROCESS-MIB::cpmCPUTotalMonIntervalValue",
						"oid" : "1.3.6.1.4.1.9.9.109.1.1.1.1.10",
						"valueType" : "GAUGE"
					}
				]
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseExpired",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when the license on the system",
				"expires after the period for which it was installed."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cwoLicenseExpired",
				"ExpireTime" : 3600,
				"Severity" : 4,
				"SubNode" : "Device",
				"Summary" : "License has expired"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseExpired",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseExpired",
				"oid" : "1.3.6.1.4.1.9.9.762.0.5"
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseRevoked",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when a requested revoke ticket",
				"is obtained. This ticket could have been requested in order to",
				"move the license to a different system."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cwoLicenseRevoked",
				"ExpireTime" : 3600,
				"Severity" : 3,
				"SubNode" : "Device",
				"Summary" : "License revoke request obtained"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseRevoked",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseRevoked",
				"oid" : "1.3.6.1.4.1.9.9.762.0.6"
			}
		},
		{
			"@objectName" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseDeleted",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when the license on the system",
				"is cleared."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cwoLicenseDeleted",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "License has been deleted/cleared"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseDeleted",
			"trap" : {
				"name" : "CISCO-WAN-OPTIMIZATION-MIB::cwoLicenseDeleted",
				"oid" : "1.3.6.1.4.1.9.9.762.0.7"
			}
		}
	]
}