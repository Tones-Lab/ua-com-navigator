		{
			"@objectName" : "ASIB-FT-MIB::asIbFtOperationalStatusChangedTrap",
			"certification" : "STANDARD",
			"description" : [
				"Signifies that iBridge stack operational status has changed."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 1) ? 1 : (($v1 == 2) ? 2 : 3)"
				},
				"EventType" : "asIbFtOperationalStatusChangedTrap",
				"ExpireTime" : {
					"eval" : "($v1 == 2) ? 86400 : 3600"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 0 : (($v1 == 2) ? 4 : 3)"
				},
				"SubNode" : "Device",
				"Summary" : "IBridge stack operational status changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ASIB-FT-MIB::asIbFtOperationalStatusChangedTrap ASIB-FT-MIB::asIbFtStackStatusOperationalStatus i 0 ASIB-FT-MIB::asIbFtStackStatusOperationalStatusInfo s EXAMPLE",
			"trap" : {
				"name" : "ASIB-FT-MIB::asIbFtOperationalStatusChangedTrap",
				"oid" : "1.3.6.1.4.1.989.1.21.1.2.0.1",
				"variables" : [
					{
						"description" : [
							"The current operational status for the iBridge stack."
						],
						"enums" : {
							"0" : "operationalStatusNone",
							"1" : "operationalStatusInService",
							"2" : "operationalStatusOutOfService",
							"3" : "operationalStatusMaintenance"
						},
						"name" : "ASIB-FT-MIB::asIbFtStackStatusOperationalStatus",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.2.1.1",
						"valueType" : "INTEGER"
					},
					{
						"description" : [
							"Provides further information on the iBridge Stack Operational Status"
						],
						"name" : "ASIB-FT-MIB::asIbFtStackStatusOperationalStatusInfo",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.2.1.2",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
		{
			"@objectName" : "ASIB-FT-MIB::asIbFtFeatureLicenceCertTrap",
			"certification" : "STANDARD",
			"description" : [
				"Trap sent to indicate a change of licence certification.",
				"i.e send when the invalid certificate is first decoded (raise alarm)",
				"and when valid certificate replaces the invalid one.",
				""
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 0) ? 2 : 1"
				},
				"EventType" : "asIbFtFeatureLicenceCertTrap",
				"ExpireTime" : {
					"eval" : "($v1 == 0) ? 86400 : 3600"
				},
				"Severity" : {
					"eval" : "($v1 == 0) ? 0 : 4"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1 == 0) ? 'Licence cerificate is invalid' : 'Licence cerificate is valid'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ASIB-FT-MIB::asIbFtFeatureLicenceCertTrap ASIB-FT-MIB::asIbFtCardFeatureLicenceCertStatus i 0 ASIB-FT-MIB::asIbFtCardFeatureLicenceStatusInfo s EXAMPLE",
			"trap" : {
				"name" : "ASIB-FT-MIB::asIbFtFeatureLicenceCertTrap",
				"oid" : "1.3.6.1.4.1.989.1.21.1.2.0.2",
				"variables" : [
					{
						"description" : [
							"The featureLicence certificate error status (true=invalid, false=valid)."
						],
						"enums" : {
							"0" : "booleanFalse",
							"1" : "booleanTrue"
						},
						"name" : "ASIB-FT-MIB::asIbFtCardFeatureLicenceCertStatus",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.4.1.51",
						"valueType" : "INTEGER"
					},
					{
						"description" : [
							"The featureLicence certificate status info in text."
						],
						"name" : "ASIB-FT-MIB::asIbFtCardFeatureLicenceStatusInfo",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.4.1.52",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
		{
			"@objectName" : "ASIB-FT-MIB::asIbFtFeatureLicenceExpiringTrap",
			"certification" : "STANDARD",
			"description" : [
				"Trap sent to indicate that the feature licence certificate is starting to expire or has expired.",
				"The supplied objects specify the status of the expiry trap - NoError, Warning or Error and the number of ",
				"days remaining until expiry.",
				"The BS must send a trap with status NoError on the successful application of a new valid Feature Licence"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "asIbFtFeatureLicenceExpiringTrap",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "($v1 == 0) ? 2 : ($v1==1) ? 3 : 4"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1 == 0) ? 'BS feature licence is not close to expiry' : (($v1 == 1) ? 'BS feature licence will be expired in $v2 days' : 'BS feature licence has expired')"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ASIB-FT-MIB::asIbFtFeatureLicenceExpiringTrap ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryStatus i 0 ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryDaysRemaining i 9999 ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryStatusInfo s EXAMPLE",
			"trap" : {
				"name" : "ASIB-FT-MIB::asIbFtFeatureLicenceExpiringTrap",
				"oid" : "1.3.6.1.4.1.989.1.21.1.2.0.3",
				"variables" : [
					{
						"description" : [
							"Used to indicate the status of the Bs feature licence expiry. ",
							"- None indicates that the licence is not close to expiry. ",
							"- Warning indicates that the licence is close to expiry with a further property specifying the number of days remaining. ",
							"- Error indicates that the licence has expired."
						],
						"enums" : {
							"0" : "asxEbsExpiryStatusNone",
							"1" : "asxEbsExpiryStatusWarning",
							"2" : "asxEbsExpiryStatusError"
						},
						"name" : "ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryStatus",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.4.1.53",
						"valueType" : "INTEGER"
					},
					{
						"description" : [
							"The number of days until the cerificate will expire. Only valid when featureLicenceCertExpiryStatus is in state Warning or Error."
						],
						"name" : "ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryDaysRemaining",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.4.1.54",
						"valueType" : "INTEGER32"
					},
					{
						"description" : [
							"The feature licence certificate expiry status info in text format."
						],
						"name" : "ASIB-FT-MIB::asIbFtCardFeatureLicenceCertExpiryStatusInfo",
						"oid" : "1.3.6.1.4.1.989.1.21.1.3.4.1.55",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
