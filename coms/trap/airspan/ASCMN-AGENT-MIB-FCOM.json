							"6" : "reasonAutoRecovery",
							"7" : "reasonDhcpAddressInvalidate",
							"8" : "reasonPtpError",
							"9" : "reasonUnexpectedSwError",
							"10" : "reasonManualResetCli",
							"11" : "reasonConfigurationChanged",
							"12" : "reasonPnp",
							"13" : "reasonAutoPci",
							"14" : "reasonResetScheduledMaintenance",
							"15" : "reasonNoTransmissionLossOfSync",
							"16" : "lowBattery",
