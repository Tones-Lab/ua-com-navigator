	"objects" : [
		{
			"@objectName" : "ASCMN-AGENT-MIB::asCmnAgentShutdownTrap",
			"certification" : "STANDARD",
			"description" : [
				"The trap is sent when the agent system controller identifies the condition critical to the system ",
				"operation e.g. overheating, and the imminent system shutdown is scheduled. ",
				"The exact reason of the shutdown is defined in ReasonCode object and additional details are given ",
				"in ReasonInfo trap object.",
				"This trap is not sent when the condition clears. Instead expect one of the start related traps (coldstart/warmstart).",
				"Default severity: Critical"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "asCmnAgentShutdownTrap",
				"ExpireTime" : 3600,
				"Severity" : 5,
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v2 ne '') ? '$v2' : 'Agent shutdown due to $v1'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ASCMN-AGENT-MIB::asCmnAgentShutdownTrap ASCMN-AGENT-MIB::asCmnAgentShutdownReasonCode i 0 ASCMN-AGENT-MIB::asCmnAgentShutdownReasonInfo s EXAMPLE",
			"trap" : {
				"name" : "ASCMN-AGENT-MIB::asCmnAgentShutdownTrap",
				"oid" : "1.3.6.1.4.1.989.1.22.1.2.0.1",
				"variables" : [
					{
						"description" : [
							"Reports the reason the board (box) executed shut down as defined in the value list."
						],
