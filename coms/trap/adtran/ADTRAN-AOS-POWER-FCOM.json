			"certification" : "STANDARD",
			"description" : [
				"This trap indicates the unit has had a change in power source, either from AC to DC or back again. The SecSinceEpoch represents the time (seconds since epoch) that the rollover occured."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "adGenAOSPowerRollover",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1==1) ? 'AOS unit power source changed to AC power' : 'AOS unit power source changed to DC power'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ADTRAN-AOS-POWER::adGenAOSPowerRollover ADTRAN-AOS-POWER::adGenAOSPowerRolloverOnAC i 1 ADTRAN-AOS-POWER::adGenAOSPwrRollOvrEvntSecSinceEpoch u 9999",
			"trap" : {
				"name" : "ADTRAN-AOS-POWER::adGenAOSPowerRollover",
				"oid" : "1.3.6.1.4.1.664.5.53.7.0.1",
				"variables" : [
					{
						"description" : [
							"Integer value specifying whether or not unit is on AC power."
						],
						"enums" : {
							"1" : "true",
							"2" : "false"
						},
						"name" : "ADTRAN-AOS-POWER::adGenAOSPowerRolloverOnAC",
