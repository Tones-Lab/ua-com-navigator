{
	"metaData" : {
		"sourceRevision" : "200805280000Z"
	},
	"objects" : [
		{
			"@objectName" : "PIM-BSR-MIB::pimBsrElectedBSRLostElection",
			"certification" : "STANDARD",
			"description" : [
				"A pimBsrElectedBSRLostElection notification should be",
				"generated when current E-BSR lost election to a new",
				"Candidate-BSR. Only an E-BSR should generate this",
				"notification.",
				"This notification is generated when",
				"pimBsrCandidateBSRElectedBSR becomes FALSE.",
				"REFERENCE RFC 5059, section 3.1"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "pimBsrElectedBSRLostElection",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "electedBSRAddress:$v2",
				"Summary" : "Current E-BSR lost election to new Candidate-BSR"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PIM-BSR-MIB::pimBsrElectedBSRLostElection PIM-BSR-MIB::pimBsrElectedBSRAddressType i 0 PIM-BSR-MIB::pimBsrElectedBSRAddress s EXAM PIM-BSR-MIB::pimBsrElectedBSRPriority u 0",
			"trap" : {
				"name" : "PIM-BSR-MIB::pimBsrElectedBSRLostElection",
				"oid" : "1.3.6.1.2.1.172.0.1",
				"variables" : [
					{
						"description" : [
							"The address type of the elected BSR."
						],
						"enums" : {
							"0" : "unknown",
							"1" : "ipv4",
							"2" : "ipv6",
							"3" : "ipv4z",
							"4" : "ipv6z",
							"16" : "dns"
						},
						"name" : "PIM-BSR-MIB::pimBsrElectedBSRAddressType",
						"oid" : "1.3.6.1.2.1.172.1.4.1.2",
						"valueType" : "INTEGER"
					},
					{
						"description" : [
							"The (unicast) address of the elected BSR. The",
							"InetAddressType is given by the",
							"pimBsrElectedBSRAddressType object."
						],
						"name" : "PIM-BSR-MIB::pimBsrElectedBSRAddress",
						"oid" : "1.3.6.1.2.1.172.1.4.1.3",
						"valueType" : "OCTET STRING"
					},
					{
						"description" : [
							"The priority value for the elected BSR for this address",
							"type. Numerically higher values for this object indicate",
							"higher priorities."
						],
						"name" : "PIM-BSR-MIB::pimBsrElectedBSRPriority",
						"oid" : "1.3.6.1.2.1.172.1.4.1.4",
						"valueType" : "UNSIGNED32"
					}
				]
			}
		},
		{
			"@objectName" : "PIM-BSR-MIB::pimBsrCandidateBSRWinElection",
			"certification" : "STANDARD",
			"description" : [
				"A pimBsrCandidateBSRWinElection notification should be",
				"generated when a C-BSR wins BSR Election. Only an",
				"E-BSR should generate this notification.",
				"This notification is generated when",
				"pimBsrCandidateBSRElectedBSR becomes TRUE.",
				"REFERENCE RFC 5059, section 3.1"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "pimBsrCandidateBSRWinElection",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "C-BSR won the BSR election"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PIM-BSR-MIB::pimBsrCandidateBSRWinElection PIM-BSR-MIB::pimBsrCandidateBSRElectedBSR i 1",
			"trap" : {
				"name" : "PIM-BSR-MIB::pimBsrCandidateBSRWinElection",
				"oid" : "1.3.6.1.2.1.172.0.2",
				"variables" : [
					{
						"description" : [
							"Whether the local router is the elected BSR for this",
							"zone."
						],
						"enums" : {
							"1" : "true",
							"2" : "false"
						},
						"name" : "PIM-BSR-MIB::pimBsrCandidateBSRElectedBSR",
						"oid" : "1.3.6.1.2.1.172.1.3.1.6",
						"valueType" : "INTEGER"
					}
				]
			}
		}
	]
}