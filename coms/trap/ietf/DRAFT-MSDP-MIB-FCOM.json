{
	"metaData" : {
		"sourceRevision" : "200103010000Z"
	},
	"objects" : [
		{
			"@objectName" : "DRAFT-MSDP-MIB::msdpEstablished",
			"certification" : "STANDARD",
			"description" : [
				"The MSDP Established event is generated when the MSDP FSM",
				"enters the ESTABLISHED state."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 1,
				"EventType" : "msdpTransitionState",
				"ExpireTime" : 3600,
				"Severity" : 0,
				"SubNode" : "Device",
				"Summary" : "State of FSM is established"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD DRAFT-MSDP-MIB::msdpEstablished DRAFT-MSDP-MIB::msdpPeerFsmEstablishedTransitions c 9999",
			"trap" : {
				"name" : "DRAFT-MSDP-MIB::msdpEstablished",
				"oid" : "1.3.6.1.3.92.1.0.1",
				"variables" : [
					{
						"description" : [
							"The total number of times the MSDP FSM transitioned into the",
							"established state."
						],
						"name" : "DRAFT-MSDP-MIB::msdpPeerFsmEstablishedTransitions",
						"oid" : "1.3.6.1.3.92.1.3.1.15",
						"valueType" : "COUNTER"
					}
				]
			}
		},
		{
			"@objectName" : "DRAFT-MSDP-MIB::msdpBackwardTransition",
			"certification" : "STANDARD",
			"description" : [
				"The MSDPBackwardTransition Event is generated when the MSDP",
				"FSM moves from a higher numbered state to a lower numbered",
				"state."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1==4) ? 1 : ($v1==1 || $v1==5) ? 2 : 3"
				},
				"EventType" : "msdpTransitionState",
				"ExpireTime" : {
					"eval" : "($v1==4) ? 3600 : ($v1==1 || $v1==5) ? 86400 : 3600"
				},
				"Severity" : {
					"eval" : "($v1==1 || $v1==5) ? 3 : ($v1==4) ? 0 : 2"
				},
				"SubNode" : "Device",
				"Summary" : "MSDP FSM state: $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD DRAFT-MSDP-MIB::msdpBackwardTransition DRAFT-MSDP-MIB::msdpPeerState i 1",
			"trap" : {
				"name" : "DRAFT-MSDP-MIB::msdpBackwardTransition",
				"oid" : "1.3.6.1.3.92.1.0.2",
				"variables" : [
					{
						"description" : [
							"The state of the MSDP TCP connection with this peer."
						],
						"enums" : {
							"1" : "inactive",
							"2" : "listen",
							"3" : "connecting",
							"4" : "established",
							"5" : "disabled"
						},
						"name" : "DRAFT-MSDP-MIB::msdpPeerState",
						"oid" : "1.3.6.1.3.92.1.3.1.3",
						"valueType" : "INTEGER"
					}
				]
			}
		}
	]
}