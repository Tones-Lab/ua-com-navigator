{
	"metaData" : {
		"sourceRevision" : "200003030000Z"
	},
	"objects" : [
		{
			"@objectName" : "VRRP-MIB::vrrpTrapNewMaster",
			"certification" : "STANDARD",
			"description" : [
				"The newMaster trap indicates that the sending agent",
				"has transitioned to 'Master' state."
			],
			"preprocessors" : [
				{
					"regex" : {
						"value" : "$oid1",
						"pattern" : "3\\.6\\.1\\.2\\.1\\.68\\.1\\.3\\.1\\.7\\.(?P<IfIndex>[0-9]+)\\.(?P<vrrpOperVrId>.*)$",
						"capture" : true
					}
				}
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "vrrpTrapNewMaster",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "vrrpOperEntry.$IfIndex:$vrrpOperVrId",
				"Summary" : "Router Transitioned to VRRP Master ( MasterIpAddr: {$v1} )"
			},
			"metaData" : {
				"certified" : false
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD VRRP-MIB::vrrpTrapNewMaster VRRP-MIB::vrrpOperMasterIpAddr.11.222 a 127.0.0.1",
			"trap" : {
				"name" : "VRRP-MIB::vrrpTrapNewMaster",
				"oid" : "1.3.6.1.2.1.68.0.1",
				"variables" : [
					{
						"description" : [
							"The master router's real (primary) IP address. This is",
							"the IP address listed as the source in VRRP advertisement",
							"last received by this virtual router."
						],
						"name" : "VRRP-MIB::vrrpOperMasterIpAddr",
						"oid" : "1.3.6.1.2.1.68.1.3.1.7",
						"valueType" : "IPADDR"
					}
				]
			}
		},
		{
			"@objectName" : "VRRP-MIB::vrrpTrapAuthFailure",
			"certification" : "STANDARD",
			"description" : [
				"A vrrpAuthFailure trap signifies that a packet has",
				"been received from a router whose authentication key",
				"or authentication type conflicts with this router's",
				"authentication key or authentication type. Implementation",
				"of this trap is optional."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 2,
				"EventType" : "vrrpTrapAuthFailure",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "($v2 == 1 || $v2 == 2 || $v2 ==3) ? 3 : 1"
				},
				"SubNode" : "vrrpOperEntry:$v1",
				"Summary" : "VRRP Failure Packet Received from {$v1} have Conflict Reason {$v2}"
			},
			"metaData" : {
				"certified" : false
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD VRRP-MIB::vrrpTrapAuthFailure VRRP-MIB::vrrpTrapPacketSrc a 127.0.0.1 VRRP-MIB::vrrpTrapAuthErrorType i 1",
			"trap" : {
				"name" : "VRRP-MIB::vrrpTrapAuthFailure",
				"oid" : "1.3.6.1.2.1.68.0.2",
				"variables" : [
					{
						"description" : [
							"The IP address of an inbound VRRP packet. Used by",
							"vrrpTrapAuthFailure trap."
						],
						"name" : "VRRP-MIB::vrrpTrapPacketSrc",
						"oid" : "1.3.6.1.2.1.68.1.5",
						"valueType" : "IPADDR"
					},
					{
						"description" : [
							"Potential types of configuration conflicts.",
							"Used by vrrpAuthFailure trap."
						],
						"enums" : {
							"1" : "invalidAuthType",
							"2" : "authTypeMismatch",
							"3" : "authFailure"
						},
						"name" : "VRRP-MIB::vrrpTrapAuthErrorType",
						"oid" : "1.3.6.1.2.1.68.1.6",
						"valueType" : "INTEGER"
					}
				]
			}
		}
	]
}