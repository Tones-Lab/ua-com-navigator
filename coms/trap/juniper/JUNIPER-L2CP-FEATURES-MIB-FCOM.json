			"certification" : "STANDARD",
			"description" : [
				"Generated when the ports Root-protect state (no-error or root-prevented) ",
				"changes."
			],
			"domain" : "FAULT",
			"preprocessors" : [
				{
					"regex" : {
						"value" : "$oid1",
						"pattern" : "\\.[0-9]+\\.(?P<dot1dStpPort>\\d+)$",
						"capture" : true
					}
				}
			],
			"event" : {
				"EventType" : "Root Protect State",
				"ExpireTime" : {
					"eval" : "($v1 == 0) ? 3600 : 86400"
				},
				"SubNode" : "dot1dStpPortEntry.$dot1dStpPort",
				"EventCategory" : {
					"eval" : "($v1 == 0) ? 1 : (($v1 == 1) ? 2 : 2)"
				},
				"Severity" : {
					"eval" : "($v1 == 0) ? 0 : (($v1 == 1) ? 3 : 2)"
				},
				"Summary" : {
					"eval" : "($v1 == 0) ? 'Port - dot1dStpPortEntry. $dot1dStpPort can be a root port.' : (($v1 == 1) ? 'Port - dot1dStpPortEntry. $dot1dStpPort is prevented from being a root port' : 'Root State unknown for the port - dot1dStpPortEntry. $dot1dStpPort')"
				}
			},
