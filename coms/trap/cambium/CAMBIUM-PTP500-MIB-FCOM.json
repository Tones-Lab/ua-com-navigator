			"@objectName" : "CAMBIUM-PTP500-MIB::channelChangeTrap",
			"certification" : "STANDARD",
			"description" : [
				"The PTP radio has changed wireless channel.",
				"This has been caused by either",
				"i) mandatory radar avoidance",
				"a DFS impulsive interference event will also be",
				"detected",
				"or",
				"ii) Dynamic Spectrum Optimization, (DSO)",
				"DSO improves link performance by selecting the",
				"most appropriate transmit and receive channels",
				"with the lowest level of interference."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "channelChangeTrap",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "PTP radio changed wireless channel"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD CAMBIUM-PTP500-MIB::channelChangeTrap CAMBIUM-PTP500-MIB::receiveChannel i 0",
			"trap" : {
