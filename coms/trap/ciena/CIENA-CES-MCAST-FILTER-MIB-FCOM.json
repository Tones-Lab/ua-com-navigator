			"@objectName" : "CIENA-CES-MCAST-FILTER-MIB::cienaCesMcastAddrOverlapNotification",
			"certification" : "STANDARD",
			"description" : [
				"A cienaCesMcastAddrOverlapNotification is sent whenever ",
				"several multicast IP addresses overlap (matches the ",
				"same L2 address).",
				"Marking it deprecated as in case of overlapping IP ",
				"addresses, no report is sent to user as the groups ",
				"are registered in any case and traffic for these ",
				"groups will be forwarded."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1==1) ? 1 : (($v1==0 || $v1==2 || $v1==7 || $v1==8 || $v1==9 || $v1==10) ? 3 : 2)"
				},
				"EventType" : "cienaCesMcastAddrOverlapNotification",
				"ExpireTime" : {
					"eval" : "($v1==1 || $v1==0 || $v1==2 || $v1==7 || $v1==8 || $v1==9 || $v1==10) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v1==1) ? 0 : (($v1==3) ? 5 : (($v1==4) ? 4 : (($v1==5 || $v1==6) ? 3 : (($v1==0) ? 1 : 2))))"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1==1) ? 'Multicast IP addresses overlap notification cleared' : 'Multicast IP addresses overlap notification occurred'"
				}
			},
			"metaData" : {
				"certified" : true
			},
