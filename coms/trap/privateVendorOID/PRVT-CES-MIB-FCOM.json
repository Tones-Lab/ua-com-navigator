		{
			"@objectName" : "PRVT-CES-MIB::prvtCESDsx1Alarm",
			"certification" : "STANDARD",
			"description" : [
				"This notification is sent when the value of a TDM port error condition statistic has exceeded the",
				"configured threshold."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "prvtCESDsx1Alarm",
				"ExpireTime" : 3600,
				"Severity" : 4,
				"SubNode" : "Device",
				"Summary" : "TDM port error exceeded configured threshold ($v2)"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PRVT-CES-MIB::prvtCESDsx1Alarm PRVT-CES-MIB::prvtCESDsx1AlarmVariable o 1.3.6.1.4.1.8072.2.1.1 PRVT-CES-MIB::prvtCESDsx1AlarmThreshold i 9999 PRVT-CES-MIB::prvtCESDsx1AlarmValue i 9999",
			"trap" : {
				"name" : "PRVT-CES-MIB::prvtCESDsx1Alarm",
				"oid" : "1.3.6.1.4.1.738.1.5.111.0.1",
				"variables" : [
					{
						"description" : [
							"The OID of the variable whose value is being monitored. Depending on the time interval, ",
							"the error condition and the ifIndex of the TDM interface, this points to an object from an entry in",
							"dsx1CurrentTable or dsx1TotalTable."
						],
						"name" : "PRVT-CES-MIB::prvtCESDsx1AlarmVariable",
						"oid" : "1.3.6.1.4.1.738.1.5.111.1.4.1.3",
						"valueType" : "OBJECTID"
					},
					{
						"description" : [
							"The alarm condition threshold for this alarm entry. The alarm is triggered if the value of the",
							"object pointed by prvtCESDsx1AlarmVariable remains above this threshold for more than 3 seconds."
						],
						"name" : "PRVT-CES-MIB::prvtCESDsx1AlarmThreshold",
						"oid" : "1.3.6.1.4.1.738.1.5.111.1.4.1.4",
						"valueType" : "INTEGER32"
					},
					{
						"description" : [
							"The value of the object pointed to by prvtCESDsx1AlarmVariable, present for notification purposes."
						],
						"name" : "PRVT-CES-MIB::prvtCESDsx1AlarmValue",
						"oid" : "1.3.6.1.4.1.738.1.5.111.1.4.1.5",
						"valueType" : "INTEGER32"
					}
				]
			}
		},
