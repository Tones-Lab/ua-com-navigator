{
	"metaData" : {
		"sourceRevision" : "200907130000Z"
	},
	"objects" : [
		{
			"@objectName" : "PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarm",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when the value of configurable",
				"attribute has been changed. The notification can be used",
				"to trigger maintenance polling of the running configuration",
				"on the device. One of the varbinds will point either to entry",
				"of the modified table (configChangeAlarmRow) or the",
				"OID of the modified scalar object "
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "prvtConfigChangeAlarm",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "The value of a configurable attribute has been changed"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarm PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarmOID o 1.3.6.1.4.1.8072.2.1.1 PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarmRow o 1.3.6.1.4.1.8072.2.1.1",
			"trap" : {
				"name" : "PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarm",
				"oid" : "1.3.6.1.4.1.738.1.5.150.0.1",
				"variables" : [
					{
						"description" : [
							"The configChangeAlarmOID specifies the OID of an object whose value has been changed."
						],
						"name" : "PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarmOID",
						"oid" : "1.3.6.1.4.1.738.1.5.150.1.1",
						"valueType" : "OBJECTID"
					},
					{
						"description" : [
							"The configChangeAlarmRow specifies the OID of the row from table, whose entry has been changed."
						],
						"name" : "PRVT-CONFIGCHANGE-MIB::prvtConfigChangeAlarmRow",
						"oid" : "1.3.6.1.4.1.738.1.5.150.1.2",
						"valueType" : "OBJECTID"
					}
				]
			}
		},
		{
			"@objectName" : "PRVT-CONFIGCHANGE-MIB::cliConfigurationChange",
			"certification" : "STANDARD",
			"description" : [
				"This notification is generated when command in configuration node is executed."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "cliConfigurationChange",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Command '$v2' executed in configuration node"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PRVT-CONFIGCHANGE-MIB::cliConfigurationChange PRVT-CONFIGCHANGE-MIB::cliConfigChangeNodePrompt s EXAMPLE PRVT-CONFIGCHANGE-MIB::cliConfigChangeCommand s EXAMPLE",
			"trap" : {
				"name" : "PRVT-CONFIGCHANGE-MIB::cliConfigurationChange",
				"oid" : "1.3.6.1.4.1.738.1.5.150.0.2",
				"variables" : [
					{
						"description" : [
							"Shows the CLI prompt."
						],
						"name" : "PRVT-CONFIGCHANGE-MIB::cliConfigChangeNodePrompt",
						"oid" : "1.3.6.1.4.1.738.1.5.150.1.3",
						"valueType" : "OCTET STRING"
					},
					{
						"description" : [
							"Shows executed command."
						],
						"name" : "PRVT-CONFIGCHANGE-MIB::cliConfigChangeCommand",
						"oid" : "1.3.6.1.4.1.738.1.5.150.1.4",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
		{
			"@objectName" : "PRVT-CONFIGCHANGE-MIB::snmpServerStatusChange",
			"certification" : "STANDARD",
			"description" : [
				"The notification shall be sent whenever the enabled/disabled status",
				"of the SNMP server changes. If the status is changed",
				"from enabled to disabled, the notification shall be sent",
				"prior to disabling the SNMP server."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v1 == 1) ? 1 : 2"
				},
				"EventType" : "snmpServerStatusChange",
				"ExpireTime" : {
					"eval" : "($v1 == 1) ? 3600 : 86400"
				},
				"Severity" : {
					"eval" : "($v1 == 1) ? 0 : 3"
				},
				"SubNode" : "Device",
				"Summary" : "SNMP server status changed to $v1"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD PRVT-CONFIGCHANGE-MIB::snmpServerStatusChange PRVT-CONFIGCHANGE-MIB::snmpServerStatus i 1",
			"trap" : {
				"name" : "PRVT-CONFIGCHANGE-MIB::snmpServerStatusChange",
				"oid" : "1.3.6.1.4.1.738.1.5.150.0.3",
				"variables" : [
					{
						"description" : [
							"Shows the enabled/disabled status of the SNMP server."
						],
						"enums" : {
							"1" : "enabled",
							"2" : "disabled"
						},
						"name" : "PRVT-CONFIGCHANGE-MIB::snmpServerStatus",
						"oid" : "1.3.6.1.4.1.738.1.5.150.1.5",
						"valueType" : "INTEGER"
					}
				]
			}
		}
	]
}