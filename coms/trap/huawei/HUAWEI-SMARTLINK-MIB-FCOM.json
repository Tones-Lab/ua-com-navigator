			"@objectName" : "HUAWEI-SMARTLINK-MIB::hwSmartLinkLinkSwitch",
			"certification" : "STANDARD",
			"description" : [
				"The SNMP trap that is generated when smart link group link status switches."
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "hwSmartLinkLinkSwitch",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "Smart link group link status ($v1) switched"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD HUAWEI-SMARTLINK-MIB::hwSmartLinkLinkSwitch HUAWEI-SMARTLINK-MIB::hwSmartLinkGcGroupStatus i 1",
			"trap" : {
				"name" : "HUAWEI-SMARTLINK-MIB::hwSmartLinkLinkSwitch",
				"oid" : "1.3.6.1.4.1.2011.5.25.42.5.2.1",
				"variables" : [
					{
						"description" : [
							"Working status of smart link group, include none, master, slave, idle and init. ",
							"none: all ports in group are inactive.",
