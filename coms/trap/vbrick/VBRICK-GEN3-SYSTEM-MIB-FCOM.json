		{
			"@objectName" : "VBRICK-GEN3-SYSTEM-MIB::vbrickRemoteLog",
			"certification" : "STANDARD",
			"description" : [
				"This trap is sent whenever a significant event occurs",
				"within the VBrick. Each event, provides details via the",
				"vbrickSysRemoteEventLogType, and vbrickSysRemoteEventLogText objects"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "vbrickRemoteLog:$v1",
				"ExpireTime" : 3600,
				"Severity" : 2,
				"SubNode" : "Device",
				"Summary" : "$v2"
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD VBRICK-GEN3-SYSTEM-MIB::vbrickRemoteLog VBRICK-GEN3-SYSTEM-MIB::vbrickSysRemoteEventLogType i 1 VBRICK-GEN3-SYSTEM-MIB::vbrickSysRemoteEventLogText s EXAMPLE",
			"trap" : {
				"name" : "VBRICK-GEN3-SYSTEM-MIB::vbrickRemoteLog",
				"oid" : "1.3.6.1.4.1.4289.3.3.1.2.0.1",
				"variables" : [
					{
						"description" : [
							"",
							"This object indicates the type of messages stored in the",
							"vbrickSysRemoteEventLogText MIB object. A value of",
							"generalConfigEvent(1) indicates that the text object contain",
							"messages related to general configuration modifications",
							"(VBAdmin, CLI, SNMP MIB). A value of frontPanelOrScriptConfigEvent(2)",
							"indicates that a configuration change was performed due to a",
							"Front Panel or Script file event. A value of alarmEvent(3) indicates",
							"that the text object contain messages regarding alarm information.",
							"A value of systemEvent(4) indicates that the text object contain",
							"messages regarding a general system error. The rtspEvent(5)",
							"indicates that the text object contain messages regarding an rtsp",
							"event. The otherEvent(6) indicates the event type falls outside",
							"the other predefined categories.",
							"This object is referenced in the remoteLog trap.",
							""
						],
						"enums" : {
							"1" : "generalConfigEvent",
							"2" : "frontPanelOrScriptConfigEvent",
							"3" : "alarmEvent",
							"4" : "systemEvent",
							"5" : "rtspEvent",
							"6" : "otherEvent"
						},
						"name" : "VBRICK-GEN3-SYSTEM-MIB::vbrickSysRemoteEventLogType",
						"oid" : "1.3.6.1.4.1.4289.3.3.1.7.16",
						"valueType" : "INTEGER"
					},
					{
						"description" : [
							"",
							"This object contains the textual description of the event.",
							"This object is referenced in the remoteLog trap.",
							""
						],
						"name" : "VBRICK-GEN3-SYSTEM-MIB::vbrickSysRemoteEventLogText",
						"oid" : "1.3.6.1.4.1.4289.3.3.1.7.17",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
