			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : {
					"eval" : "($v3==2) ? 1 : (($v3==3) ? 2 : 3)"
				},
				"EventType" : "swPowerStatusChangeTrap",
				"ExpireTime" : {
					"eval" : "($v3==2) ? 3600 : (($v3==3) ? 86400 : 3600)"
				},
				"Severity" : {
					"eval" : "($v3==2) ? 0 : (($v3==3) ? 4 : 1)"
				},
				"SubNode" : "PowerUnitIndex:$v1",
				"Summary" : {
					"eval" : "($v2==1) ? 'Internal power status changed to $v3' : 'External power status changed to $v3'"
				}
			},
			"metaData" : {
				"certified" : true
			},
