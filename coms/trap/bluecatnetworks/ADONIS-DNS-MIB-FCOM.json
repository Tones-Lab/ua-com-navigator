{
	"metaData" : {
		"sourceRevision" : "200810010000Z"
	},
	"objects" : [
		{
			"@objectName" : "ADONIS-DNS-MIB::trapDNSDaemon",
			"certification" : "STANDARD",
			"description" : [
				"DNS daemon has stopped running or a zone transfer failed"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "trapDNSDaemon",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "($v1==0) ? 4 : 3"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1==0) ? 'DNS daemon is not running' : '$v2'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ADONIS-DNS-MIB::trapDNSDaemon ADONIS-DNS-MIB::dnsDaemonRunning i 1 ADONIS-DNS-MIB::dnsDaemonZoneTransferFailure s EXAMPLE",
			"trap" : {
				"name" : "ADONIS-DNS-MIB::trapDNSDaemon",
				"oid" : "1.3.6.1.4.1.13315.100.101.2.1.1",
				"variables" : [
					{
						"description" : [
							"Current running state of the DNS daemon.",
							"0 - Not Running",
							"1 - Running"
						],
						"name" : "ADONIS-DNS-MIB::dnsDaemonRunning",
						"oid" : "1.3.6.1.4.1.13315.100.101.1.1.1.1",
						"valueType" : "INTEGER32"
					},
					{
						"description" : [
							"The last zone transfer failure desciption which includes the time, the zone name, the master address and the explaination"
						],
						"name" : "ADONIS-DNS-MIB::dnsDaemonZoneTransferFailure",
						"oid" : "1.3.6.1.4.1.13315.100.101.1.1.1.8",
						"valueType" : "OCTET STRING"
					}
				]
			}
		},
		{
