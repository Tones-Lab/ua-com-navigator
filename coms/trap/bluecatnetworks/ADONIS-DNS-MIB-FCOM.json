		{
			"@objectName" : "ADONIS-DNS-MIB::trapDHCPDaemon",
			"certification" : "STANDARD",
			"description" : [
				"DHCP daemon has stopped running or",
				"the number of assigned IPS in one of the subnets has reached",
				"to the alert level or DHCP failover state changes"
			],
			"domain" : "FAULT",
			"event" : {
				"EventCategory" : 3,
				"EventType" : "trapDHCPDaemon",
				"ExpireTime" : 3600,
				"Severity" : {
					"eval" : "($v1==0) ? 4 : 3"
				},
				"SubNode" : "Device",
				"Summary" : {
					"eval" : "($v1==0) ? 'DHCP daemon is not running' : 'Either DHCP failover state changed to $v3, or IP addresses on subnet $v2 at alert level'"
				}
			},
			"metaData" : {
				"certified" : true
			},
			"method" : "trap",
			"test" : "$SNMPTRAPCMD ADONIS-DNS-MIB::trapDHCPDaemon ADONIS-DNS-MIB::dhcpDaemonRunning i 1 ADONIS-DNS-MIB::dhcpDaemonSubnetAlert a 192.0.2.1 ADONIS-DNS-MIB::dhcpFailOverState i 1",
			"trap" : {
				"name" : "ADONIS-DNS-MIB::trapDHCPDaemon",
				"oid" : "1.3.6.1.4.1.13315.100.101.2.4.1",
				"variables" : [
					{
						"description" : [
							"Current running state of the DHCP daemon.",
							"0 - Not Running",
							"1 - Running"
						],
						"name" : "ADONIS-DNS-MIB::dhcpDaemonRunning",
						"oid" : "1.3.6.1.4.1.13315.100.101.1.2.1.1",
						"valueType" : "INTEGER32"
					},
					{
						"description" : [
							"The IP address which has to be alerted for."
						],
						"name" : "ADONIS-DNS-MIB::dhcpDaemonSubnetAlert",
						"oid" : "1.3.6.1.4.1.13315.100.101.1.2.1.2",
						"valueType" : "IPADDR"
					},
					{
						"description" : [
							"Current state of the DHCP failover.",
							"1 - startup",
							"2 - normal",
							"3 - communications interrupted",
							"4 - partner down",
							"5 - potential conflict",
							"6 - recover",
							"7 - paused",
							"8 - shutdown",
							"9 - recover done",
							"254 - recover wait"
						],
						"name" : "ADONIS-DNS-MIB::dhcpFailOverState",
						"oid" : "1.3.6.1.4.1.13315.100.101.1.2.1.4",
						"valueType" : "INTEGER32"
					}
				]
			}
		},
