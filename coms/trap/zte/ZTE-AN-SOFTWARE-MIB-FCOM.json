							"3" : "fileServerConnectFailed",
							"4" : "fileServerLoginFailed",
							"5" : "fileServerPathError",
							"6" : "fileServerProtocolTypeError",
							"7" : "deviceCheckFailed",
							"255" : "otherErrors"
